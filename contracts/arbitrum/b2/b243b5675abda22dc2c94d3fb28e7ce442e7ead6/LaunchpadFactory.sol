pragma solidity ^0.8.0;

abstract contract WithClaimer is AdminableUpgradeable, GeneralIDO {
    using SafeMath for uint256;
    using SafeERC20 for IERC20;

    struct Claim {
        uint256 unlockTime; // unix time
        uint256 percent; // 6 decimals: 1.783% = 1783000
    }

    Claim[] public claims;
    bool public isPaused = false;
    uint256 public claimedTokens;
    IERC20 public token;

    mapping(address => uint256) public claimed;
    mapping(address => mapping(uint256 => uint256)) public isClaimed;

    event Claimed(
        address indexed account,
        uint256 amount,
        uint256 percent,
        uint256 claimIdx
    );
    event ClaimReleased(uint256 percent, uint256 newTime, uint256 claimIdx);
    event ClaimDelayed(uint256 percent, uint256 newTime, uint256 claimIdx);
    event ClaimingPaused(bool status);

    function setClaimInfo(
        address _token,
        uint256[] memory times,
        uint256[] memory percents
    ) public onlyOwnerOrAdmin {
        token = IERC20(_token);
        uint256 totalPercent;
        for (uint256 i = 0; i < times.length; i++) {
            require(percents[i] > 0, "Claimer: 0% is not allowed");
            require(times[i] > 0, "Claimer: time must specified");
            require(
                times[i] > getEndTime(),
                "Claimer: time must be after end time"
            );
            claims.push(Claim(times[i], percents[i]));
            totalPercent += percents[i];
        }
        require(totalPercent == 100e6, "Claimer: Invalid total percent");
    }

    function getAllClaims() public view returns (Claim[] memory) {
        return claims;
    }

    function getRemainingAccountAmount(address account)
        external
        view
        returns (uint256)
    {
        return balances[account] - claimed[account];
    }

    function getClaimableAccountAmount(address account)
        external
        view
        returns (uint256)
    {
        uint256 totalClaimable;
        for (uint256 i = 0; i < claims.length; i++) {
            if (claims[i].unlockTime < block.timestamp) {
                totalClaimable += getClaimAmount(i, account);
            }
        }

        return totalClaimable - claimed[account];
    }

    function isClaimable(uint256 claimIdx) internal view returns (bool) {
        return claims[claimIdx].unlockTime < block.timestamp;
    }

    function getClaimAmount(uint256 claimIdx, address account)
        internal
        view
        returns (uint256)
    {
        if (balances[account] == 0) {
            return 0;
        }

        return (balances[account] * claims[claimIdx].percent) / 100e6;
    }

    function pauseClaiming(bool status) external onlyOwnerOrAdmin {
        isPaused = status;
        emit ClaimingPaused(status);
    }

    function setToken(address _token) external onlyOwnerOrAdmin {
        require(
            claims[0].unlockTime > block.timestamp,
            "Claimer: Claim started"
        );
        token = IERC20(_token);
    }

    function delayClaim(uint256 claimIdx, uint256 newUnlockTime)
        external
        onlyOwner
    {
        require(claimIdx < claims.length, "Claimer: Out of bounds index");
        Claim storage _claim = claims[claimIdx];

        require(
            newUnlockTime > block.timestamp,
            "Claimer: Time must be in future"
        );
        require(newUnlockTime > _claim.unlockTime, "Claimer: Invalid new Time");
        _claim.unlockTime = newUnlockTime;
        emit ClaimDelayed(_claim.percent, _claim.unlockTime, claimIdx);
    }

    function releaseClaim(uint256 claimIdx) external onlyOwner {
        require(claimIdx < claims.length, "Claimer: Out of bounds index");
        Claim storage _claim = claims[claimIdx];

        require(
            _claim.unlockTime > block.timestamp,
            "Claimer: Claim already released"
        );
        _claim.unlockTime = block.timestamp;
        emit ClaimReleased(_claim.percent, _claim.unlockTime, claimIdx);
    }

    function claim(address account, uint256 idx) external {
        require(idx < claims.length, "Claimer: Out of bounds index");
        require(isClaimed[account][idx] == 0, "Claimer: Already claimed");
        require(
            claims[idx].unlockTime < block.timestamp,
            "Claimer: Not claimable"
        );
        require(balances[account] > 0, "Claimer: No Allocation");
        require(!isPaused, "Claimer: Claiming paused");

        uint256 claimAmount = getClaimAmount(idx, account);
        require(claimAmount > 0, "Claimer: Amount is zero");

        claimedTokens += claimAmount;
        claimed[account] += claimAmount;
        isClaimed[account][idx] = claimAmount;

        token.safeTransfer(account, claimAmount);

        emit Claimed(account, claimAmount, claims[idx].percent, idx);
    }

    function getClaims(address account)
        external
        view
        returns (
            uint256[] memory,
            uint256[] memory,
            uint256[] memory,
            bool[] memory,
            uint256[] memory
        )
    {
        uint256 len = claims.length;
        uint256[] memory times = new uint256[](len);
        uint256[] memory percents = new uint256[](len);
        uint256[] memory amount = new uint256[](len);
        bool[] memory _isClaimable = new bool[](len);
        uint256[] memory claimedAmount = new uint256[](len);

        for (uint256 i = 0; i < len; i++) {
            times[i] = claims[i].unlockTime;
            percents[i] = claims[i].percent;
            amount[i] = getClaimAmount(i, account);
            _isClaimable[i] = block.timestamp > claims[i].unlockTime;
            claimedAmount[i] = isClaimed[account][i];
        }

        return (times, percents, amount, _isClaimable, claimedAmount);
    }
}

// File contracts/LaunchpadIDO/LaunchpadIDO.sol

pragma solidity =0.8.4;

contract LaunchpadIDO is
    AdminableUpgradeable,
    ReentrancyGuard,
    Timed,
    GeneralIDO,
    Withdrawable,
    WithLimits,
    WithLevelsSale,
    WithCancelable,
    WithClaimer
{
    using SafeERC20 for IERC20;
    using SafeMath for uint256;
    using EnumerableSet for EnumerableSet.AddressSet;

    string public id;
    uint256 public tokensSold;
    uint256 public raised;
    EnumerableSet.AddressSet internal participants;
    uint256 public firstPurchaseBlockN;
    uint256 public lastPurchaseBlockN;

    event TokensPurchased(
        address indexed beneficiary,
        uint256 value,
        uint256 amount
    );

    function initialize(
        string memory _id,
        uint256 _startTime,
        uint256 _duration,
        uint256 _rate,
        uint256 _tokensForSale,
        address _fundToken,
        address _priceFeed,
        bool _levelsEnabled
    ) public {
        require(stringsEqual(id, ""), "already initialized.");
        AdminableUpgradeable.__initialize();
        _General_Ido_Init(_rate, _tokensForSale, _priceFeed);
        _Timed_Init(_startTime, _duration);
        _Withdraw_Init(_fundToken);
        id = _id;
        levelsEnabled = _levelsEnabled;
    }

    function buyTokens(
        uint256 amount,
        uint256 userWeight,
        bytes32[] memory merkleProof
    ) public payable ongoingSale nonReentrant notCanceled {
        require(
            !fundByTokens,
            "Sale: presale is funded by tokens but value is missing"
        );

        internalBuyTokens(msg.value, amount, userWeight, merkleProof);
    }

    /**
     * The fund token must be first approved to be transferred by presale contract for the given "value".
     */
    function buyTokens(
        uint256 value,
        uint256 amount,
        uint256 userWeight,
        bytes32[] memory merkleProof
    ) public ongoingSale nonReentrant notCanceled {
        require(fundByTokens, "Sale: funding by tokens is not allowed");
        require(
            fundToken.allowance(msg.sender, address(this)) >= value,
            "Sale: fund token not approved"
        );

        internalBuyTokens(value, amount, userWeight, merkleProof);

        fundToken.safeTransferFrom(msg.sender, address(this), value);
    }

    function internalBuyTokens(
        uint256 value,
        uint256 amount,
        uint256 userWeight,
        bytes32[] memory merkleProof
    ) private {
        address account = _msgSender();
        // Verify the merkle proof.
        bytes32 node = keccak256(abi.encodePacked(account, userWeight));
        userWeight = MerkleProof.verify(merkleProof, merkleRoot, node)
            ? userWeight
            : 0;
        (
            uint256 maxAllocation,
            uint256 minAllocation
        ) = checkAccountAllowedToBuy(userWeight);

        require(value > 0, "Sale: value is 0");

        uint256 requiredValue = calculateNativeRequiredAmount(amount);
        require(amount > 0, "Sale: amount is 0");
        require(msg.value >= requiredValue, "Sale: low balance.");
        tokensSold += amount;
        balances[account] += amount;
        require(amount >= minAllocation, "Sale: amount is too small");
        require(
            maxAllocation == 0 || balances[account] <= maxAllocation,
            "Sale: amount exceeds max allocation"
        );
        require(tokensSold <= tokensForSale, "Sale: cap reached");

        raised = raised + value;
        participants.add(msg.sender);

        // Store the first and last block numbers to simplify data collection later
        if (firstPurchaseBlockN == 0) {
            firstPurchaseBlockN = block.number;
        }
        lastPurchaseBlockN = block.number;

        uint256 extra = msg.value.sub(requiredValue);
        if (extra > 0) payable(msg.sender).transfer(extra);

        emit TokensPurchased(account, value, amount);
    }

    function checkAccountAllowedToBuy(uint256 userWeight)
        private
        view
        returns (uint256, uint256)
    {
        uint256 levelAllocation = userWeight;

        // Public sale with no levels
        if (!levelsEnabled) {
            return (maxSell, minSell);
        }

        // Check user level if levels enabled and user was not whitelisted
        if (levelsEnabled) {
            // If opened for all levels, just return the level allocation without checking user weight
            if (levelsOpenAll()) {
                (, uint256 fcfsAllocation) = getUserLevelState(userWeight);
                require(
                    fcfsAllocation > 0,
                    "Sale: user does not have FCFS allocation"
                );

                return (fcfsAllocation, minSell);
            }

            require(
                levelAllocation > 0,
                "Sale: user has no level allocation or level is too low"
            );

            return (levelAllocation, 0);
        }

        revert("Sale: unreachable state");
    }

    function getParticipants()
        public
        view
        returns (address[] memory, uint256[] memory)
    {
        address[] memory users = participants.values();
        uint256[] memory amounts = new uint256[](participants.length());
        for (uint256 i = 0; i < participants.length(); i++) {
            amounts[i] = balances[users[i]];
        }
        return (users, amounts);
    }

    function max(uint256 a, uint256 b) private pure returns (uint256) {
        return a > b ? a : b;
    }
}

