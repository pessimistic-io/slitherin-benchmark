pragma solidity =0.8.4;

abstract contract Adminable is Ownable, AccessControl {
    constructor() {
        _setupRole(DEFAULT_ADMIN_ROLE, _msgSender());
    }

    modifier onlyOwnerOrAdmin() {
        require(
            owner() == _msgSender() ||
                hasRole(DEFAULT_ADMIN_ROLE, _msgSender()),
            "Adminable: caller is not the owner or admin"
        );
        _;
    }
}

// File contracts/Factory/factory.sol

pragma solidity =0.8.4;

contract LaunchpadFactory is Adminable {
    address public libraryAddress;
    struct IdoInfo {
        LaunchpadIDO ido;
        string dataUri;
    }

    IdoInfo[] public idos;
    event IDOCreated(address ido);

    constructor() {
        libraryAddress = address(new LaunchpadIDO());
    }

    function setLibraryAddress(address _libraryAddress) public onlyOwner {
        libraryAddress = _libraryAddress;
    }

    function deployIDO(
        string memory _id,
        uint256 _startTime,
        uint256 _duration,
        uint256 _rate,
        uint256 _tokensForSale,
        address _fundToken,
        address _priceFeed,
        bool _levelsEnabled,
        string memory dataUri
    ) public onlyOwner {
        address clone = Clones.clone(libraryAddress);
        LaunchpadIDO(clone).initialize(
            _id,
            _startTime,
            _duration,
            _rate,
            _tokensForSale,
            _fundToken,
            _priceFeed,
            _levelsEnabled
        );
        idos.push(IdoInfo(LaunchpadIDO(clone), dataUri));
        emit IDOCreated(clone);
    }

    function setIdo(LaunchpadIDO _ido, string memory _dataUri)
        external
        onlyOwnerOrAdmin
    {
        idos.push(IdoInfo(_ido, _dataUri));
    }

    function updateDataUri(uint256 index, string memory _dataUri)
        external
        onlyOwnerOrAdmin
    {
        require(index < idos.length, "Index out of bounds");
        idos[index].dataUri = _dataUri;
    }

    function getAllIdos() public view returns (IdoInfo[] memory) {
        return idos;
    }
}