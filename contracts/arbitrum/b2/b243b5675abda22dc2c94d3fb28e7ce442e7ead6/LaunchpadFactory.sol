abstract contract WithLevelsSale is
    AdminableUpgradeable,
    Timed,
    GeneralIDO,
    WithLimits
{
    using SafeMath for uint256;
    bytes32 public merkleRoot;
    string public dataUri;
    bool public levelsEnabled = true;
    bool public forceLevelsOpenAll = false;

    event LevelsEnabled(bool status);
    event SaleOpenedForAllLevels(bool status);

    function levelsOpenAll() public view returns (bool) {
        return forceLevelsOpenAll || isFcfsTime();
    }

    function setTierUsersRoot(bytes32 _root, string memory _dataUri)
        public
        onlyOwnerOrAdmin
    {
        require(!isLive(), "Sale: Cannot set, sale is live");
        require(_root != 0, "Sale: Cannot set empty root");
        merkleRoot = _root;
        dataUri = _dataUri;
    }

    /**
     * Return:  multiplier, allocation,.
     *
     * User is a winner when:
     * - winners were picked for the level
