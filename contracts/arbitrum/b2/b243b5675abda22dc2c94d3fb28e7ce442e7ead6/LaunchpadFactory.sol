pragma solidity =0.8.4;

abstract contract GeneralIDO is AdminableUpgradeable, Timed {
    using SafeMath for uint256;
    // Actual rate is: rate / 1e18
    // 6.123456 actual rate = 6123456000000 specified rate
    uint256 public rate;
    uint256 public tokensForSale;

    mapping(address => uint256) public balances;

    AggregatorV3Interface internal priceFeed;

    event RateChanged(uint256 newRate);
    event CapChanged(uint256 newRate);

    function _General_Ido_Init(
        uint256 _rate,
        uint256 _tokensForSale,
        address _priceFeed
    ) internal {
        rate = _rate;
        tokensForSale = _tokensForSale;
        priceFeed = AggregatorV3Interface(_priceFeed);
    }

    function balanceOf(address account) external view returns (uint256) {
        return balances[account];
    }

    function setTokensForSale(uint256 _tokensForSale)
        external
        onlyOwnerOrAdmin
    {
        require(
            !isLive() || _tokensForSale > tokensForSale,
            "Sale: Sale is live, cap change only allowed to a higher value"
        );
        tokensForSale = _tokensForSale;
        emit CapChanged(tokensForSale);
    }

    function calculatePurchaseAmount(uint256 purchaseAmountWei)
        public
        view
        returns (uint256)
    {
        uint256 nativeRate = getRateNative();
        return purchaseAmountWei.mul(1e18).div(nativeRate);
    }

    function calculatePurchaseAmountUSD(uint256 purchaseAmountUSDWei)
        public
        view
        returns (uint256)
    {
        int256 price = getNativePriceUSD();
        return
            calculatePurchaseAmount(
                purchaseAmountUSDWei.mul(1e8).div(uint256(price))
            );
    }

    function calculateNativeRequiredAmount(uint256 _tokenAmount)
        public
        view
        returns (uint256)
    {
        uint256 nativeRate = getRateNative();

        return _tokenAmount.mul(nativeRate).div(1e18);
    }

    function setRate(uint256 newRate) public onlyOwnerOrAdmin {
        require(!isLive(), "Sale: Sale is live, rate change not allowed");
        rate = newRate;
        emit RateChanged(rate);
    }

    function setPriceAggregator(address _priceFeed) public onlyOwnerOrAdmin {
        priceFeed = AggregatorV3Interface(_priceFeed);
    }

    // returns the Native currency price of token
    function getRateNative() public view returns (uint256) {
        int256 price = getNativePriceUSD();
        uint256 cPrice = (rate.mul(1e8)).div(uint256(price));
        return cPrice;
    }

    function getNativePriceUSD() public view returns (int256) {
        (, int256 price, , , ) = priceFeed.latestRoundData();
        return price;
    }

    function stringsEqual(string memory a, string memory b)
        internal
        pure
        returns (bool)
    {
        return keccak256(abi.encodePacked(a)) == keccak256(abi.encodePacked(b));
    }
}

