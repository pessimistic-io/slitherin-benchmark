    }

    function addTowhitelist(address[] calldata users, uint256[] calldata amounts) external onlyOwner {
        require(users.length == amounts.length, "Lengths do not match");
        for (uint256 i = 0; i < users.length; i++) {
            whitelist[users[i]] = amounts[i];
        }
    }

    function claim(uint256[] calldata tokenIDs) external {
        require(istokenclaim, "Claim Stop!");
        uint256 claimableamount=0;
        require(goblintribe.isApprovedForAll(msg.sender, address(this)), "Contract is not authorized to transfer NFTs");
        for (uint256 i=0; i<tokenIDs.length; i++){
            uint256 tokenID=tokenIDs[i];
            address tokenowner=goblintribe.ownerOf(tokenID);
            require(tokenowner==msg.sender, "Is not your NFT!");
            uint256 amount;
            if(tokenID<4001){
                amount=2025625000000*1e6;
            }else if(tokenID<8001){
                amount=1782550000000*1e6;
            }else if(tokenID<12001){
                amount=1458450000000*1e6;
            }else if(tokenID<16001){
                amount=1296400000000*1e6;
            }else{
                amount=1134350000000*1e6;
            }
            claimableamount+=amount;
            goblintribe.transferFrom(msg.sender, DEAD, tokenID);
        }
        uint256 timespend=block.timestamp-claimAtTimestamp;
        uint256 claimableamounttimeloss=claimableamount-claimableamount*timespend*timefee/timefeeDenominator;
        require(claimableamounttimeloss > 0, "You can claim 0 token else");
        uint256 beforetokenminted = tokenminted;
        uint256 aftertotalamount = beforetokenminted+claimableamounttimeloss;
        require(aftertotalamount<fixtotalSupply,"Out of Supply!");
        _mint(_msgSender(), claimableamounttimeloss);
        tokenminted+=claimableamounttimeloss;
        emit Claimed(msg.sender, claimableamounttimeloss);
    }

    function whitelistclaim() external {
        uint256 amount = whitelist[msg.sender];
        require(amount > 0, "Not in whitelist or already claimed");
        uint256 beforetotalamount = totalSupply();
        uint256 aftertotalamount = beforetotalamount+amount;
        require(aftertotalamount<fixtotalSupply,"Out of Supply!");
        whitelist[msg.sender] = 0;
        _mint(_msgSender(), amount);
        tokenminted+=amount;
        emit Claimed(msg.sender, amount);
    }


    function burnRemainToken() external onlyOwner{
        uint256 remaintokenamount=fixtotalSupply-tokenminted;
