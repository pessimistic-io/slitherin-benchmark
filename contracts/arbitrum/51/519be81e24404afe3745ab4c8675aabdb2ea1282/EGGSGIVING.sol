*/

struct Ticket {
  bool active;
  bool randomized;
  uint randomRequestId;
  uint random;
  bool drawn;
  bool consolated;
  uint consolationItemId;
  uint freeTickets;
  uint freeClaims;
  uint tokenPrice;
  uint maxTickets; // max tickets that can be bought
  uint ticketsBought; // tickets that have been bought
}

struct AwardNFT {
  address addr;
  uint tokenId;
  bool active;
}

contract EGGSGIVING is 
  ERC721Enumerable, 
  UserAccessible,
  ERC721Holder,
  TokenAccessible
{
  using SafeERC20 for IERC20;
  using Strings for uint256;

  uint _tokenCount = 0;
  string public _baseUri;

  Ticket[] tickets;
  mapping (uint => uint[]) ticketToTokens; // ticketId => tokenId[]
  mapping (uint => AwardNFT[]) ticketToAwards; // ticketId => Awards
  mapping (uint => mapping(uint => uint)) awardToTicket; // ticketId => tokenId => awardId
  mapping (uint => uint) tokenToTicket; // tokenId => ticketId

  mapping (uint => mapping (address => bool)) claimedFreeTicket; // ticketId => address => claimed

  IRandomizer public randomizer;
  IERC20 public paymentToken;
  IItems public items;

  bool enabled = true;

  uint[] private tokenContractIds;

  address public paymentReceiver;

  uint public minimumTokens = 2; // treshold to claim
  uint public maximumPurchaseAmount = 5; // maximum tokens to buy per tx

  event TokenWon (address to, uint tokenId, uint ticketId, uint awardId);
  event TokenLost (address to, uint tokenId, uint ticketId);

  constructor(address userAccessAddr, address tokenAccessAddr) 
    ERC721("Tickets for EGGSGIVING", "TICKEGG")
    UserAccessible(userAccessAddr)
    TokenAccessible(tokenAccessAddr)
  {}

  modifier paymentReady () {
    require(paymentReceiver != address(0) && paymentToken != IERC20(address(0)), 'PAYMENT_NOT_SETUP');
    _;
  }

  modifier onlyEOA () {
    require(msg.sender == tx.origin, 'NOPE_FREN');
    _;
  }

  modifier isEnabled () {
    require(enabled, "NOT_ENABLED");
    _;
  }

  modifier raffleReady () {
    require(randomizer != IRandomizer(address(0)));
    _;
  }

  modifier ticketsAvailable (uint ticketId) {
    Ticket memory ticket = tickets[ticketId];
    require(ticket.active && !ticket.randomized && !ticket.drawn, 'INACTIVE_TICKET');
    _;
  }

  function ownsAtLeastTokens (address owner, uint minimum) public view returns (bool) {
    require(tokenAccess != ITokenAccess(address(0)), 'INVALID_TOKEN_META');
    require(tokenContractIds.length > 0, 'CONTRACTS_NOT_SET');
    uint balance = 0;
    IERC721 token;
    for (uint8 i = 0; i < tokenContractIds.length; i++) {
      token = IERC721(tokenAccess.getAddress(tokenContractIds[i]));
      balance += token.balanceOf(owner);
      if (balance >= minimum) return true;
    }
    return false;
  }

  function awards (uint ticketId) public view returns (AwardNFT[] memory) {
    return ticketToAwards[ticketId];
  }

  function contractIds () public view returns (uint[] memory) {
    return tokenContractIds;
  }

  function getAward (uint ticketId, uint awardId) public view returns (AwardNFT memory) {
    return ticketToAwards[ticketId][awardId];
  }

  function getTicket (uint ticketId) public view returns (Ticket memory) {
    return tickets[ticketId];
  }

  function getTokenCount (uint ticketId) public view returns (uint) {
    return ticketToTokens[ticketId].length;
  }

  function canClaimFree (address sender, uint ticketId) public view returns (bool) {
    return !claimedFreeTicket[ticketId][sender] && ownsAtLeastTokens(sender, minimumTokens);
  }

  function buyTickets (uint ticketId, uint amount) 
    public 
    onlyEOA
    isEnabled
    paymentReady
    ticketsAvailable(ticketId)
  {
    Ticket storage ticket = tickets[ticketId];
    require(ticket.tokenPrice > 0, 'PRICE_NOT_SET');
    require(amount > 0 && amount <= maximumPurchaseAmount, "INVALID_AMOUNT");
    if (ticket.maxTickets > 0) {
      require(ticket.ticketsBought + amount <= ticket.maxTickets, 'OUT_OF_SELLABLE_TICKETS');
    }
    uint paymentAmount = amount * ticket.tokenPrice;
    require(paymentToken.allowance(msg.sender, address(this)) >= paymentAmount, 'ALLOWANCE_LOW');
    for (uint8 i = 0; i < amount; i++) {
      _mintTicket(msg.sender, ticketId);
    }
    ticket.ticketsBought += amount;
    paymentToken.safeTransferFrom(msg.sender, paymentReceiver, paymentAmount);
  }

  function freeTicket (uint ticketId) 
    public
    onlyEOA
    isEnabled
    paymentReady
    ticketsAvailable(ticketId)
  {
    Ticket storage ticket = tickets[ticketId];
    require(ticket.freeClaims < ticket.freeTickets, 'OUT_OF_FREE');
    require(ownsAtLeastTokens(msg.sender, minimumTokens), 'MIN_TOKENS_REQUIRED');
    require(!claimedFreeTicket[ticketId][msg.sender], 'ALREADY_CLAIMED');
    ticket.freeClaims += 1;
    claimedFreeTicket[ticketId][msg.sender] = true;
    _mintTicket(msg.sender, ticketId);
  }

  function redeem (uint[] calldata tokenIds) 
    public 
    isEnabled 
  {
    uint tokenId;
    for (uint i = 0; i < tokenIds.length; i++) {
      tokenId = tokenIds[i];
      require(ownerOf(tokenId) == msg.sender, 'INVALID_OWNER');
      _redeem(tokenId);
    }
  }

  function burn (uint tokenId) 
    public
    isEnabled
    onlyRole(keccak256("BURN_TICKET_ROLE"))
  { 
    _burn(tokenId);
  }

  function mintTickets (address to, uint ticketId, uint amount) 
    public 
    onlyRole(keccak256("MINT_TICKET_ROLE"))
  {
    Ticket memory ticket = tickets[ticketId];
    require(!ticket.randomized && !ticket.drawn, 'INACTIVE_TICKET');
    for (uint8 i = 0; i < amount; i++) {
      _mintTicket(to, ticketId);
    }
  }

  function ownerTransferTo(address to, address addr, uint tokenId) public onlyAdmin {
    IERC721(addr).safeTransferFrom(address(this), to, tokenId);
  }

  function randomize (uint ticketId) public raffleReady onlyAdmin {
    Ticket storage ticket = tickets[ticketId];
    require(!ticket.randomized, 'ALREADY_RANDOMIZED');
    ticket.randomized = true;
    ticket.randomRequestId = randomizer.requestRandomNumber();
  }

  function draw (uint ticketId) public onlyAdmin {
    Ticket storage ticket = tickets[ticketId];
    require(randomizer.isRandomReady(ticket.randomRequestId), 'RANDOM_NOT_READY');
    require(!ticket.drawn, 'ALREADY_DRAWN');
    ticket.random = randomizer.revealRandomNumber(ticket.randomRequestId);
    ticket.drawn = true;
  }

  function setConsolationPrize (uint ticketId, uint itemId) public onlyAdmin {
    Ticket storage ticket = tickets[ticketId];
    ticket.consolated = true;
    ticket.consolationItemId = itemId;
  }

  function removeConsolationPrize (uint ticketId) public onlyAdmin {
    Ticket storage ticket = tickets[ticketId];
    ticket.consolated = false;
    ticket.consolationItemId = 0;
  }

  function addContractId (uint contractId) public onlyAdmin { tokenContractIds.push(contractId); }
  function removeContractIdAtIndex (uint index) public onlyAdmin {
    tokenContractIds[index] = tokenContractIds[tokenContractIds.length - 1];
    tokenContractIds.pop();
  }

  function addAward (uint ticketId, address contractAddr, uint tokenId) public onlyAdmin {
    require(_contractOwnsToken(contractAddr, tokenId), 'CONTRACT_NOT_OWNER');
    require(tickets[ticketId].active, 'INACTIVE_TICKET');
    ticketToAwards[ticketId].push(
      AwardNFT({
        addr: contractAddr,
        tokenId: tokenId,
        active: true
      })
    );
  }
  function updateAward (uint ticketId, uint awardId, bool newActive) public onlyAdmin {
    AwardNFT storage award = ticketToAwards[ticketId][awardId];
    if (newActive == true) {
      require(_contractOwnsToken(award.addr, award.tokenId), 'CONTRACT_NOT_OWNER');
    }
    award.active = newActive;
  }

  function addTicket (bool active) public onlyAdmin {
    tickets.push(
      Ticket({
        active: active,
        randomRequestId: 0,
        randomized: false,
        random: 0,
        drawn: false,
        consolated: false,
        consolationItemId: 0,
        freeTickets: 0,
        freeClaims: 0,
        tokenPrice: 0,
        maxTickets: 0,
        ticketsBought: 0
      })
    );
  }
  function updateTicket (uint ticketId, bool newActive, uint newFreeTickets) public onlyAdmin {
    tickets[ticketId].active = newActive;
    tickets[ticketId].freeTickets = newFreeTickets;
  }

  function setTicketPrice (uint ticketId, uint newTicketPrice) public onlyAdmin {
    tickets[ticketId].tokenPrice = newTicketPrice;
  }

  function setEnabled (bool state) public onlyAdmin {
    enabled = state;
  }

  function setMaxTicketsBought (uint ticketId, uint newMaxTicketsBought) public onlyAdmin {
    tickets[ticketId].maxTickets = newMaxTicketsBought;
  }

  function transferAward (uint ticketId, uint awardId, address to) public onlyAdmin {
    AwardNFT memory award = ticketToAwards[ticketId][awardId];
    require(!award.active, 'AWARD_ACTIVE');
    _transferAward(award, to);
  }

  function setMinimumTokens (uint newMinimumTokens) public onlyAdmin { minimumTokens = newMinimumTokens; }
  function setPaymentReceiver (address newPaymentReceiver) public onlyAdmin { paymentReceiver = newPaymentReceiver; }
  function setPaymentToken (address newTokenAddr) public onlyAdmin { paymentToken = IERC20(newTokenAddr); }
  function setRandomizer (address newRandomizerAddr) public onlyAdmin { randomizer = IRandomizer(newRandomizerAddr); }
  function setItems (address newItemsAddr) public onlyAdmin { items = IItems(newItemsAddr); }
  function setURI (string memory newUri) public onlyAdmin { _baseUri = newUri; }
  function setTokenAccess (address newTokenAccess) public onlyAdmin { _setTokenAccess(newTokenAccess); }
  function setMaximumPurchaseAmount (uint newMaximumPurchaseAmount) public onlyAdmin { maximumPurchaseAmount = newMaximumPurchaseAmount; }

  function _mintTicket (address to, uint ticketId) private {
    uint tokenId = _tokenCount;
    ticketToTokens[ticketId].push(tokenId);
    tokenToTicket[tokenId] = ticketId;
    _safeMint(to, tokenId);
    _tokenCount++;
  }

  function _redeem (uint tokenId) private {
    uint ticketId = tokenToTicket[tokenId];
    Ticket storage ticket = tickets[ticketId];
    require(ticket.randomized && ticket.drawn, 'TICKET_NOT_READY');
    AwardNFT[] storage ticketAwards = ticketToAwards[ticketId];
    require(ticketAwards.length > 0, 'NO_AWARDS');
    AwardNFT storage award;
    uint winTokenId;
    bool won;
    address owner = ownerOf(tokenId);
    for (uint awardId = 0; awardId < ticketAwards.length; awardId++) {
      award = ticketAwards[awardId];
      if (!award.active) continue;
      winTokenId = winningTokenForAward(ticketId, awardId);
      if (winTokenId == tokenId) {
        won = true;
        _transferAward(award, owner);
        award.active = false;
        emit TokenWon(msg.sender, tokenId, ticketId, awardId);
      }
    }
    if (!won) {
      if (ticket.consolated) {
        items.mint(owner, ticket.consolationItemId, 1);
      }
      emit TokenLost(msg.sender, tokenId, ticketId);
    }
    _burn(tokenId);
  }

  function _contractOwnsToken(address addr, uint tokenId) private view returns (bool) {
    return IERC721(addr).ownerOf(tokenId) == address(this);
  }

  function _transferAward (AwardNFT memory award, address to) private {
    IERC721(award.addr).safeTransferFrom(address(this), to, award.tokenId);
  }

  function tokenURI(uint256 tokenId) public view override returns (string memory) {
    super.tokenURI(tokenId); // check if token exists
    uint ticketId = tokenToTicket[tokenId];
    return string(abi.encodePacked(_baseUri, ticketId.toString(), '.json'));
  }

  function winningTokenForAward (uint ticketId, uint awardId) public view returns (uint) {
    uint random = uint(keccak256(abi.encode(tickets[ticketId].random, awardId)));
    return ticketToTokens[ticketId][
      random % ticketToTokens[ticketId].length
    ];
  }

}