contract lotteryAIAPE is Ownable{
    using EnumerableSet for EnumerableSet.AddressSet;
    using SafeCast for int256;
    AggregatorV3Interface internal priceFeed;

    EnumerableSet.AddressSet private _callers;

    address public pairAddress;

    IERC20 public bonusToken; // ARB
    uint256 public totalBonus = 0;

    address public tokenAddress;

    uint256 public minBonus;
    uint256 public maxBonus;

    uint256 public ticketEntryPrice = 92 * 1 ** 18; // 1 dollar

    uint256 public latestBonusTimestamp = 0;
    uint256 public lastRoundPassTime = 30*60;

    uint256 internal nonce = 0;
    uint256 public roundTicketCount = 0;
    uint256 public totalRound = 0;
    struct winnerDetail {
        address user;
        uint256 totalRewards;
    }
    mapping(uint256 => winnerDetail) public winnerList;
    mapping(uint256 => uint256) public randomList;

    struct Tick {
        address user;
        uint256 amount;
        uint256 size; // total amount / tickentryprice
        uint256 time;
    }
    Tick[] public ticks;

    uint256 public allTicksCount = 0;
    struct AllTick {
        address user;
        uint256 amount;
        uint256 size; // total amount / tickentryprice
        uint256 time;
    }
    AllTick[] public allTicks;

    constructor(address pairAddress_, address _priceFeedAddress, IERC20 bonusToken_ , address tokenAddress_, uint256 maxBonus_, uint256 minBonus_) {
        pairAddress = pairAddress_;
        minBonus = minBonus_;
        maxBonus = maxBonus_;
        bonusToken = bonusToken_;
        priceFeed = AggregatorV3Interface(_priceFeedAddress);
        tokenAddress = tokenAddress_;
    }
    function lottery() public onlyCaller{
        require(getTickLength() > 0 , "Error Tick Length = 0 #3");
        uint256 bal = bonusToken.balanceOf(address(this));
        if(bal < minBonus) {
            return;
        }
        require(latestBonusTimestamp + lastRoundPassTime < block.timestamp , "Time error");
        if(bal > maxBonus){
            bal = maxBonus;
        }

        uint256 getRandom = random();
        address winner = ticks[getRandom].user;
        totalRound +=1;
        winnerList[totalRound].user = winner; 
        winnerList[totalRound].totalRewards = bal; 
        randomList[totalRound] = getRandom;

        bonusToken.transfer(winner,bal);

        totalBonus += bal;
        roundTicketCount = 0;
        delete ticks;
        nonce +=1;
        latestBonusTimestamp = block.timestamp;
    }
    function tradeEvent(address user, uint256 amount) public onlyCaller{
        if(amount * getTokenPrice(pairAddress,tokenAddress,18) / (10 ** 18) >= ticketEntryPrice){
            roundTicketCount += 1;
            uint256 ticketSize = ((amount * getTokenPrice(pairAddress,tokenAddress,18)) / (10**18)) / ticketEntryPrice;
            ticks.push(Tick({user: user, amount: amount, size: ticketSize, time: getBlockTimestamp()}));
            allTicks.push(AllTick({user: user, amount: amount, size: ticketSize, time: getBlockTimestamp()}));
            allTicksCount += 1;
        }
        
    }
    function getTickLength() public view returns(uint256){
        return ticks.length;
    }
    function random() public view returns (uint256) {
        uint256 randomnumber = uint(keccak256(abi.encodePacked(block.difficulty, block.timestamp, block.number, nonce))) % getTickLength();
        return randomnumber;
    }

    function getTokenPrice(address _pairAddress, address _tokenAddress, uint256 tokenDecimal) public view returns (uint256) {
        IToken pair = IToken(_pairAddress);
        

        (, int256 latestPrice, , , ) = priceFeed.latestRoundData();

        uint256 price = pair.getAmountOut(10**18 , _tokenAddress);
        price = price / 10 ** (18-tokenDecimal);
        price = price * latestPrice.toUint256();
        return price / 10 ** 8;
    }

    function getEthPrice() internal view returns (int256) {
        (, int256 price, , ,) = priceFeed.latestRoundData();
        return price;
    }
    function getBonusTokenAmount() public view returns (uint256){
        return bonusToken.balanceOf(address(this));
    }
    function getBlockTimestamp() public view returns(uint256){
        return block.timestamp;
    }
    /*
    onlyOwner
     */
    function setTicketEntryPrice(uint256 val) public onlyOwner{
        ticketEntryPrice = val;
    }
    function setMinBouns(uint256 val) public onlyOwner {
        require(val < maxBonus, "bad val");
        minBonus = val;
    }

    function setMaxBouns(uint256 val) public onlyOwner {
        require(val > minBonus, "bad val");
        maxBonus = val;
    }

    function setLastRoundPassTime(uint256 val) public onlyOwner {
        lastRoundPassTime = val;
    }
    function withdraw() public onlyOwner{
        uint256 bal = bonusToken.balanceOf(address(this));
        bonusToken.transfer(msg.sender,bal);
    }
    function addCaller(address val) public onlyOwner() {
        require(val != address(0), "Jackpot: val is the zero address");
        _callers.add(val);
    }

    function delCaller(address caller) public onlyOwner returns (bool) {
        require(caller != address(0), "Jackpot: caller is the zero address");
        return _callers.remove(caller);
    }

    function getCallers() public view returns (address[] memory ret) {
        return _callers.values();
    }
    modifier onlyCaller() {
        require(_callers.contains(_msgSender()), "onlyCaller");
        _;
    }
}