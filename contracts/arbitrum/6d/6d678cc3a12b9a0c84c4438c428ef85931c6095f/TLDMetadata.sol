contract TLDMetadata is Initializable, TLDMetadataState, ITLDMetadata {
    using StringsUpgradeable for uint256;

    function initialize() external initializer {
        TLDMetadataState.__TLDMetadataState_init();
    }

    function tokenURI(uint256 _tokenURI) public view override returns (string memory) {
        uint256 _tokenIndex = _tokenURI + 1;
        return string(abi.encodePacked(baseURI, '/', _tokenIndex.toString(), '.json'));
    }

    function setBaseURI(string calldata _baseURI) public onlyAdminOrOwner {
        baseURI = _baseURI;
    }

    function setProvenance(string calldata _provenance)
        public
        onlyAdminOrOwner
    {
        provenance = _provenance;
    }
}

