contract Factory is Ownable {
    using ClonesUpgradeable for address;

    event New721Contract(address indexed owner, address indexed token);

    address public immutable implementation;
    mapping(address => address[]) public ownerToContracts;

    uint256 public fee = 0.00095 ether;
    address public fileStore;

    constructor(address _fs) {
        _initializeOwner(msg.sender);
        fileStore = _fs;
        implementation = address(new GenerativeArt());
        GenerativeArt.SaleInfo memory saleInfo = GenerativeArt.SaleInfo(type(uint256).max, 0, payable(address(0)), 0, 0, type(uint256).max);
        GenerativeArt.TokenInfo memory tokenInfo = GenerativeArt.TokenInfo("", "");
        GenerativeArt(implementation).initialize("IMPL", "IMPL", saleInfo, tokenInfo, fileStore);
    }

    function createContract(
        string calldata name,
        string calldata symbol,
        GenerativeArt.SaleInfo memory _sale,
        GenerativeArt.TokenInfo memory _token
    ) external returns (address clone) {
        _sale.feeRecipient = payable(owner());
        _sale.fee = fee;

        clone = implementation.clone();

        GenerativeArt(clone).initialize(name, symbol, _sale, _token, fileStore);
        Ownable(clone).transferOwnership(msg.sender);

        ownerToContracts[msg.sender].push(clone);

        emit New721Contract(msg.sender, clone);
    }

}