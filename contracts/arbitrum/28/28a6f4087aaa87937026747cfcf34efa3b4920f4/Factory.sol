contract GenerativeArt is OwnableUpgradeable, ERC721EnumerableUpgradeable {
    using StringsUpgradeable for uint256;

    struct TokenInfo {
        string script;
        string description;
    }

    struct SaleInfo {
        uint256 price;
        uint256 fee;
        address payable feeRecipient;
        uint256 supply;
        uint256 start;
        uint256 end;
    }

    TokenInfo public tokenInfo;
    SaleInfo public saleInfo;

    IFileStore public fileStore;

    mapping(uint256 => bytes32) public seeds;

    function initialize(string calldata _name, string calldata _symbol, SaleInfo calldata _sale, TokenInfo calldata _token, address _fs) public initializer {
        __Ownable_init();
        __ERC721_init(_name, _symbol);

        saleInfo = _sale;
        tokenInfo = _token;

        fileStore = IFileStore(_fs);
    }

    function isSaleLie() public view returns(bool) {
        return block.timestamp > saleInfo.start && block.timestamp < saleInfo.end && totalSupply() < saleInfo.supply;
    }

    function mint(uint256 _amount) public payable {
        require(block.timestamp > saleInfo.start, "Sale has not started");
        require(block.timestamp < saleInfo.end, "Sale has ended");
        require(totalSupply() + _amount <= saleInfo.supply, "Exceeds maximum supply");
        uint256 priceWithFee = saleInfo.price + saleInfo.fee;
        require(msg.value == priceWithFee * _amount, "Value sent is below the price");

        saleInfo.feeRecipient.transfer(saleInfo.fee * _amount);
        payable(owner()).transfer(address(this).balance);

        for (uint256 i = 0; i < _amount; i++) {
            seeds[totalSupply() + 1] = keccak256(abi.encodePacked(block.timestamp, msg.sender, totalSupply() + 1));
            _mint(msg.sender, totalSupply() + 1);
        }
    }

    function tokenURI(uint256 id) public view returns (string memory){
        require(_exists(id), "Token does not exist");
        return string.concat(
            "data:application/json;base64,",
            Base64.encode(
                abi.encodePacked(
                    string.concat(
                        "{\"name\":\"",
                        name(),
                        " #",
                        id.toString(),
                        "\",\"description\":\"",
                        tokenInfo.description,
                        "\",\"image\":\"https://api.spellbook.art/",
                        block.chainid.toString(),
                        "/",
                        StringsUpgradeable.toHexString(address(this)),
                        "/",
                        id.toString(),
                        "\",\"animation_url\":\"",
                        artwork(seeds[id]),
                        "\"}"
                    )
                )
            )
        );
    }

    function artwork(bytes32 seed) public view returns(string memory) {
        return string.concat(
            "data:text/html;base64,",
            Base64.encode(
                abi.encodePacked(
                    string.concat(
                        "<!DOCTYPE html><html style=\"height: 100%;\"><body style=\"margin: 0;display: flex;justify-content: center;align-items: center;height: 100%;\">",
                        p5js(),
                        getScript(seed),
                        "</body></html>"
                    )
                )
            )
        );
    }

    function getScript(bytes32 seed) public view returns(string memory) {
        return string.concat(
            "<script src=\"data:text/javascript;base64,",
            Base64.encode(
                abi.encodePacked(
                    string.concat(
                        "let tokenHash = ",
                        uint256(seed).toString(),
                        ";\n"
                    ),
                    Base64.decode(tokenInfo.script)
                )
            ),
            "\"></script>"
        );
    }

    function p5js() public view returns(string memory) {
        return string.concat(
            "<script type=\"text/javascript+gzip\" src=\"data:text/javascript;base64,",
            fileStore.getFile("p5-v1.5.0.min.js.gz").read(),
            "\"></script>",
            "<script src=\"data:text/javascript;base64,",
            fileStore.getFile("gunzipScripts-0.0.1.js").read(),
            "\"></script>"
        );
    }

    function royaltyInfo(uint256, uint256 salePrice) public view returns (address, uint256) {
        uint256 royaltyAmount = (salePrice * 750) / 10000;

        return (owner(), royaltyAmount);
    }

}

