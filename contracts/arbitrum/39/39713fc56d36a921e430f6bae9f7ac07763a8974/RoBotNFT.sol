    function addAdmin(address _user) public onlyOwner{
        require(Admin[_user] == false, "the user already admin");
        Admin[_user] = true;
        emit AddAdmin(_user);
    }
    function removeAdmin(address _user) public onlyOwner{
        require(Admin[_user] == true, "the user not admin");
        Admin[_user] = false;
        emit RemoveAdmin(_user);
    }
    modifier onlyAdmin() {
        require(Admin[msg.sender] == true, "the caller not admin" );
        _;
    }
    function stakeBoosterConsume(address _user, uint256 _index) public onlyAdmin{
        require(boosterNumber[_user][_index] > 0 , "the Booster amount not enough ");
        boosterNumber[_user][_index] = boosterNumber[_user][_index].sub(1);
    }
