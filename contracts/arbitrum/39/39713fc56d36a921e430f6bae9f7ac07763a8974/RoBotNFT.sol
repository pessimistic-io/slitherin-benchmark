contract RoBotNFT is ERC20, Ownable{
    using SafeMath for uint256;
    using SafeERC20 for IERC20;
    using Address for address;
    string public constant name = "RoBot NFT";
    string public constant symbol = "RoBot NFT";
    uint8 public constant decimals = 18;
    bool public status = true;
    mapping(address =>mapping(uint256 => uint256)) public boosterNumber;
    //合成5种卡片需要的碎片数量
    uint256 public boosterZero = 20 * 10 ** 18;
    uint256 public boosterOne= 28 * 10 ** 18;
    uint256 public boosterTwo = 45 *10 ** 18;
    uint256 public boosterThree = 70 * 10 ** 18;
    uint256 public boosterFour = 87 * 10 ** 18;
    mapping(address => bool) public Admin;
    event ExecuteBox(address _user, uint256 _result);
    event AddPool(address _user, uint256 _index);
    event MixBooster(address _user, uint256 _index);
    event AddAdmin(address _user);
    event RemoveAdmin(address _user);
    struct BoxInfo {
        IERC20 token;
        IERC20 rewardToken;
        uint256 amount;
        uint256 result1;
        uint256 result2;
        uint256 result3;
        uint256 result4;
        uint256 result5;
        uint256 participatePeople;
    }
    BoxInfo[] public Boxinfo;

    modifier validatePool(uint256 _pid) {
        require(_pid < Boxinfo.length, " Boxinfo exists?");
        _;
    }
    function setStatus(bool _status) onlyOwner public {
        status = _status;
    }
    function setPoolInfo(uint256 _pid, IERC20 _rewardToken, uint256 _amount, uint256 _result1, uint256 _result2,  uint256 _result3, uint256 _result4, uint256 _result5) public onlyOwner{
        BoxInfo storage info = Boxinfo[_pid];
        info.rewardToken = _rewardToken;
        info.amount = _amount;
        info.result1 = _result1;
        info.result2 = _result2;
        info.result3 = _result3;
        info.result4 = _result4;
        info.result5 = _result5;
    }
    
    function setSplinterNumber(uint256 _boosterZero, uint256 _boosterOne, uint256 _boosterTwo, uint256 _boosterThree, uint256 _boosterFour) onlyOwner public {
        boosterZero = _boosterZero;
        boosterOne = _boosterOne;
        boosterTwo = _boosterTwo;
        boosterThree = _boosterThree;
        boosterFour = _boosterFour;
    }

    function withdrawToken(address _token, address payable to) public onlyOwner{
        if (_token == address(0)) {
            to.transfer(address(this).balance);
        }else{
            IERC20(_token).safeTransfer(to, IERC20(_token).balanceOf(address(this)));
        }
    }

    function addPool(IERC20 _token, IERC20 _rewardToken, uint256 _amount, uint256 _result1, uint256 _result2,  uint256 _result3, uint256 _result4, uint256 _result5) public onlyOwner {
        Boxinfo.push(BoxInfo({
            token: _token,
            rewardToken: _rewardToken,
            amount: _amount,
            result1: _result1,
            result2: _result2,
            result3: _result3,
            result4: _result4,
            result5: _result5,
            participatePeople: 0

        }));
        emit AddPool(msg.sender, Boxinfo.length.sub(1));
    }
    //盲盒抽奖
    function executeBox(uint256 _pid) public payable returns(uint256) {
        require(status, "the BlindBox Already paused");
        address _owner = msg.sender;
        require(!_owner.isContract(), "the caller is contract");
        BoxInfo storage info = Boxinfo[_pid];
        info.token.safeTransferFrom(msg.sender, address(this), info.amount);
        uint256 index = uint256(keccak256(abi.encodePacked(blockhash(block.number), msg.sender, block.timestamp, _pid))).mod(100);
        uint256 _answer = executeRusult(index, info);
        info.participatePeople++;
        emit ExecuteBox(msg.sender, _answer);
        return _answer;
    }

    function executeRusult(uint256 _index, BoxInfo storage info) internal returns(uint256){
        uint256 _value;
        uint256 _result;
        if(_index >= 0 && _index <= 39) {
            _value = info.result1;
            _result = 1;
        }
        if(_index >=40 && _index <= 64) {
            _value = info.result2;
            _result = 2;
        }
        if(_index >=65 && _index <= 84){
            _value = info.result3;
            _result = 3;
        }
        if(_index >=85 && _index <= 94){
            _value = info.result4;
            _result = 4;
        }
        if(_index >=95 && _index <= 99){
            _value = info.result5;
            _result = 5;
        }
        _mint(msg.sender, _value);
        return _result;
    }
    receive() external payable {}

    //碎片合成
    function mixBooster(uint256 _index) public returns(bool){
        require(_index < 5, "without this index");
