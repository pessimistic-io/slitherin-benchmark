            z = time.sub(tt) / 1 days;
            z = z == 0 ? 1 : z;
            z = z*la;
        }
        uint256 y = z/10;
        bool w = y > 100;
        uint256 x = w ? 0 : y;
        return (inv*x/100, w);
    }
