// File contracts/IntentExecutor.sol

// Original license: SPDX_License_Identifier: UNLICENSED
pragma solidity ^0.8.19;



contract IntentExecutor is IStargateReceiver {
    using SafeERC20Upgradeable for IERC20Upgradeable;

    enum Status {
        Pending,
        Executed
    }

    struct SourceIntentData {
        uint16 destinationChainId; 
        address sourceToken;
        uint256 amount;
        address destinationToken;
        uint256 destinationNativeAmount;
        address swapper;
        bytes swapData;
    }

    struct DestinationIntentData {
        uint256 amount;
        address user;
        address destinationToken;
        Status status;
        address bridgeToken;
        uint256 receivedAmount;
    }

    uint256 public constant MAX_BPS = 10000;

    uint256 public constant GAS_REQUIRED_ON_DESTINATION = 600000;

    ///////////////////////////////////////////////////
    /////////////// STG Configuration ////////////////
    /////////////////////////////////////////////////

    // minimum quantity of tokens to recieve on the destination chain. (99.94% of the amount sent)
    uint256 public constant minimumAmountInDestination = 9994;

    // destinationChainID => true
    mapping (uint16 => bool) public destinationConfigured;

    // destinationChainId => destinationPoolId (ex: USDC pool ID)
    mapping (uint16 => uint256) public destinationPoolId;

    // destinationChainId => destinationAddress (ex: address of IntentExecutor contract)
    mapping (uint16 => address) public destinationExecutor;

    // chain id of the chain where the intent was created
    uint16 public chainId; 

    // pool id of the pool where the intent was created (ex: USDC pool ID)
    uint256 public poolId;

    // address of the token that this chain sends and receives (ex: USDC)
    address public bridgeToken;

    // address of the STG router contract
    address public stgRouter;

    // ID of the next intent
    uint256 public nextIntentId;

    // Intents for processing on destination
    mapping (uint16 => mapping (uint256 => DestinationIntentData)) public intents;

    event IntentCreated(
        uint16 destinationChainId,
        address sourceToken, 
        address sourceBridgeToken,
        address destionationToken,
        uint256 sourceTokenAmount,
        uint256 destinationNativeAmount,
        uint256 intentId
    );

    event IntentPending(
        uint16 sourceChainId,
        uint256 intentId
    );
    event IntentExecuted(
        uint16 sourceChainId,
        uint256 intentId
    );

    constructor(
        uint16 _chainId,
        uint256 _poolId,
        address _bridgeToken,
        address _stgRouter
    ) payable {
        chainId = _chainId;
        poolId = _poolId;
        bridgeToken = _bridgeToken;
        stgRouter = _stgRouter;

        nextIntentId = 0;
    }

    function setDestination(uint16 _destinationChainId, uint256 _destinationPoolId, address _destinationExecutor) external {
        destinationConfigured[_destinationChainId] = true;
        destinationPoolId[_destinationChainId] = _destinationPoolId;
        destinationExecutor[_destinationChainId] = _destinationExecutor;
    }

    function sendIntent(
        SourceIntentData calldata _intentData
    ) external payable {
        require(destinationConfigured[_intentData.destinationChainId], "Destination not configured");
        require(_intentData.amount > 0, "Amount must be greater than 0");

        _receiveAsset(_intentData.sourceToken, _intentData.amount);
        uint amount = _intentData.amount;
        if (_intentData.sourceToken != bridgeToken) {
            amount = _swapTokens(_intentData.sourceToken, bridgeToken, _intentData.amount, _intentData.swapper, _intentData.swapData);
        }

        _approveAssetForTransfer(bridgeToken, amount);

        uint256 destinationAmountMin = (amount * minimumAmountInDestination) / MAX_BPS;
        
        bytes memory destinationPayload = abi.encode(_intentData.destinationToken, msg.sender, nextIntentId, _intentData.destinationNativeAmount);
        uint256 fee = _getCrossChainTransferFee(_intentData.destinationChainId, destinationPayload, _intentData.destinationNativeAmount);

        uint256 _destinationPoolId = destinationPoolId[_intentData.destinationChainId];

        _transferCrossChain(
            _intentData.destinationChainId, 
            poolId,
            _destinationPoolId, 
            amount, 
            msg.sender, 
            fee,
            destinationAmountMin,
            _intentData.destinationNativeAmount,
            destinationPayload
        );

        emit IntentCreated(
            _intentData.destinationChainId,
            _intentData.sourceToken, 
            bridgeToken,
            _intentData.destinationToken,
            _intentData.amount,
            _intentData.destinationNativeAmount,
            nextIntentId
        );

        nextIntentId++;
    }

    function _swapTokens(
        address fromToken,
        address toToken,
        uint256 amount,
        address swapper,
        bytes memory swapData
    ) internal returns (uint256) {
        IERC20Upgradeable fromAsset = IERC20Upgradeable(fromToken);
        fromAsset.safeApprove(swapper, amount);

        IERC20Upgradeable asset = IERC20Upgradeable(toToken);
        uint256 previousBalance = asset.balanceOf(address(this));
        
        // no native token support for swap
        // use swapper.call{value: nativeValue}(_swap.callData)
        swapper.call(swapData); 

        uint256 currentBalance = asset.balanceOf(address(this));
        require(currentBalance - previousBalance > 0, "Swap failed");
        return currentBalance - previousBalance;
    }

    function _receiveAsset(address _token, uint256 _amount) internal {
        IERC20Upgradeable asset = IERC20Upgradeable(_token);
        asset.safeTransferFrom(msg.sender, address(this), _amount);
    }

    function _approveAssetForTransfer(address _token, uint256 _amount) internal {
        IERC20Upgradeable asset = IERC20Upgradeable(_token);
        asset.safeApprove(stgRouter, _amount);
    }

    function _getCrossChainTransferFee(
        uint16 _destinationChainId,
        bytes memory destinationPayload,
        uint256 _destinationNativeAmount
    ) view internal returns (uint256 fee) {
        IStargateRouter router = IStargateRouter(stgRouter);
        (fee, ) = router.quoteLayerZeroFee(
            _destinationChainId,
            1,
            abi.encodePacked(destinationExecutor[_destinationChainId]),
            destinationPayload,
            IStargateRouter.lzTxObj(GAS_REQUIRED_ON_DESTINATION, _destinationNativeAmount, abi.encodePacked(destinationExecutor[_destinationChainId]))
        );
    }

    function getCrossChainTransferFee(
        uint16 _destinationChainId,
        uint256 _destinationNativeAmount
    ) view external returns (uint256 fee) {
        bytes memory destinationPayload = abi.encode(address(0), msg.sender, nextIntentId, _destinationNativeAmount);
        return _getCrossChainTransferFee(_destinationChainId, destinationPayload, _destinationNativeAmount);
    }

    // function payload(address token) external view returns (bytes memory) {
    //     return abi.encode(token, msg.sender, nextIntentId, 0);
    // }

    function _transferCrossChain(
        uint16 _destinationChainId,
        uint256 _sourcePoolId,
        uint256 _destinationPoolId,
        uint256 _amount,
        address _toAddress,
        uint256 _fee,
        uint256 _destinationAmountMin,
        uint256 _destinationNativeAmount,
        bytes memory destinationPayload
    ) internal {
        IStargateRouter router = IStargateRouter(stgRouter);
        address dstAddr = destinationExecutor[_destinationChainId];
        router.swap{ value: _fee }(
            _destinationChainId,
            _sourcePoolId,
            _destinationPoolId,
            payable(msg.sender),
            _amount,
            _destinationAmountMin,
            IStargateRouter.lzTxObj(GAS_REQUIRED_ON_DESTINATION, _destinationNativeAmount, abi.encodePacked(dstAddr)),
            abi.encodePacked(dstAddr),
            destinationPayload
        );
    }


    /////////////////////////////////////////////////////
    /////////////// Intent Receiver ////////////////////
    ///////////////////////////////////////////////////

    function sgReceive(
        uint16 _srcChainId,              // the remote chainId sending the tokens
        bytes memory _srcAddress,        // the remote Bridge address
        uint256 _nonce,                  
        address _token,                  // the token contract on the local chain
        uint256 amountLD,                // the qty of local _token contract tokens  
        bytes memory payload
    ) external payable {
        require(_token == bridgeToken, "Invalid token");
        require(msg.sender == stgRouter, "Only STG Router can call this function");
        (
            address tokenAddress,
            address to,
            uint256 intentId,
            uint256 nativeAmount
        ) = abi.decode(payload, (address, address, uint256, uint256));

        Status _status = Status.Pending;
        uint256 receivedAmount = 0;

        if (bridgeToken == tokenAddress) {
            _status = Status.Executed;

            IERC20Upgradeable source = IERC20Upgradeable(bridgeToken);
            uint256 amount = source.balanceOf(address(this));
            require(amount > 0, "Amount must be greater than 0");
            receivedAmount = amount;
            source.safeTransfer(to, amount);

            emit IntentExecuted(
                _srcChainId,
                intentId
            );
        }

        payable(to).transfer(nativeAmount);
        intents[_srcChainId][intentId] = DestinationIntentData({
            amount: amountLD,
            user: to,
            destinationToken: tokenAddress,
            status: _status,
            bridgeToken: bridgeToken,
            receivedAmount: receivedAmount
        });

        if (bridgeToken != tokenAddress) {
            emit IntentPending(
                _srcChainId,
                intentId
            );
        }
    }

    function executeIntent(
        uint16 sourceChainId,
        uint256 intentId,
        address swapper,
        bytes memory swapData
    ) payable public {
        DestinationIntentData storage intent = intents[sourceChainId][intentId];
        require(intent.status == Status.Pending, "Intent already executed");

        IERC20Upgradeable source = IERC20Upgradeable(bridgeToken);
        uint256 amount = source.balanceOf(address(this));

        require(amount > 0, "Amount must be greater than 0");

        amount = _swapTokensAndTransfer(bridgeToken, intent.destinationToken, intent.amount, intent.user, swapper, swapData);
        intent.status = Status.Executed;
        intent.receivedAmount = amount;

        emit IntentExecuted(
            sourceChainId,
            intentId
        );
    }

    function _swapTokensAndTransfer(
        address fromToken,
        address toToken,
        uint256 amount,
        address recipient,
        address swapper,
        bytes memory swapData
    ) internal returns (uint256) {
        IERC20Upgradeable fromAsset = IERC20Upgradeable(fromToken);
        IERC20Upgradeable toAsset = IERC20Upgradeable(toToken);
        
        fromAsset.safeApprove(swapper, amount);
        uint256 toAssetPreviousBalance = toAsset.balanceOf(address(this));
        swapper.call(swapData); 
        uint256 toAssetCurrentBalance = toAsset.balanceOf(address(this));
        require(toAssetCurrentBalance - toAssetPreviousBalance > 0, "Swap failed");

        uint256 swappedAmount = toAssetCurrentBalance - toAssetPreviousBalance;
        toAsset.safeTransfer(recipient, swappedAmount);
        return swappedAmount;
    }

    receive() external payable {}
}