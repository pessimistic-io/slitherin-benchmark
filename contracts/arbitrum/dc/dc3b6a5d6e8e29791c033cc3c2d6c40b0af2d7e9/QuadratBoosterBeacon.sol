pragma solidity 0.8.13;

import {     UpgradeableBeacon } from "./UpgradeableBeacon.sol";

/// @title QuadratV2Beacon stores vault implementation.
contract QuadratBoosterBeacon is UpgradeableBeacon {
    // solhint-disable-next-line no-empty-blocks
    constructor(address implementation_, address owner_)
        UpgradeableBeacon(implementation_)
    {
        require(implementation_ != address(0), "ZIA");
        require(owner_ != address(0), "ZOA");
        _transferOwnership(owner_);
    }
}

