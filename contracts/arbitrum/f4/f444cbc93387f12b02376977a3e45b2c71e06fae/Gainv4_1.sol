    /**
     * @dev GAIN tokens are rebased w.r.t Dai tokens.
     *
     * Requirements:
     * - only owner can rebase.
     *
     * Emits a {Rebase} event.
     */

    function rebase() external virtual {
        // Check if enableRebaseWhitelist is true
        require(
            enableRebaseWhitelist,
            "GainTokenv4: Whitelist is not enabled."
        );
        // Check if msg.sender is the owner or is whitelisted
        require(
            msg.sender == owner() || whitelisted[msg.sender],
            "GainTokenv4: You are not authorized to execute this function."
        );
        require(
            rebaseLockStatus &&
                (lastRebaseTimestampSec + intervalRebasePeriod) <=
                block.timestamp,
            "GainTokenv4: You can not rebase"
        );

        epoch += 1; // increasing rebase count
        int256 _supplyDelta = 0;
        lastRebaseTimestampSec = block.timestamp; // last rebase timestamp

        uint112 _reserve0;
        uint112 _reserve1;
        uint112 _temp;

        (_reserve0, _reserve1, ) = Gain_Dai.getReserves();

        // interchange the token reserve according to token position.
        if (Gain_Dai.token1() == address(this)) {
            _temp = _reserve0;
            _reserve0 = _reserve1;
            _reserve1 = _temp;
        }
        // Positive Rebase
        if (RTRR() > TRR) {
            uint256 storedDexPoolRebase = 0;
            uint256 reservePercentage = uint256(
                (totalReserves() * 10 ** 3) - (_totalSupply * TRR)
            );
            uint256 percentage = (reservePercentage * 10 ** 20) /
                uint256(_totalSupply * TRR);

            uint256 totalSupply = uint256(
                IERC20Upgradeable(address(this)).totalSupply()
            );
            uint256 _data = percentage * totalSupply;

            _supplyDelta = int256(((_data * 10 ** 3) / DSM) / 10 ** 20);
            if (_supplyDelta != 0) {
                storedDexPoolRebase = getHolderRebase(DEXPoolAddress);
                _totalSupply = _totalSupply.add(uint256(_supplyDelta));

                // Round down _totalSupply to the nearest readable integer
                _totalSupply = (_totalSupply / 10 ** 18) * 10 ** 18;

                // Check if _totalSupply should be less than Max Supply
                if (_totalSupply > MAX_SUPPLY) {
                    _totalSupply = MAX_SUPPLY;
                }

                _gonsPerFragment = TOTAL_GONS.div(_totalSupply);
                _mint(DAOAllocationAddress, storedDexPoolRebase);
            }
        }
        // Negative Rebase
        if (RTRR() < TRR) {
            uint256 reservePercentage = uint256(
                (_totalSupply * TRR) - (totalReserves() * 10 ** 3)
            );
            uint256 percentage = (reservePercentage * 10 ** 20) /
                uint256(_totalSupply * TRR);

            uint256 totalSupply = uint256(
                IERC20Upgradeable(address(this)).totalSupply()
            );
            uint256 _data = percentage * totalSupply;

            _supplyDelta = -int256(_data / 10 ** 20);
            if (_supplyDelta != 0) {
                _burn(DEXPoolAddress, getHolderRebase(DEXPoolAddress));
                _totalSupply = _totalSupply.sub(uint256(_supplyDelta.abs()));

                // Round down/up _totalSupply to the nearest readable integer
                _totalSupply = (_totalSupply / 10 ** 18) * 10 ** 18;

                // Check if _totalSupply should be less than Max Supply
                if (_totalSupply > MAX_SUPPLY) {
                    _totalSupply = MAX_SUPPLY;
                }

                _gonsPerFragment = TOTAL_GONS.div(_totalSupply);
            }
        }

        Gain_Dai.sync(); // syncs the gain tokens in uniswap pair address

        emit Rebase(epoch, _totalSupply, _supplyDelta);
    }

    // @dev overrides the _transfer function by using the Adaptive Pool Fragment Modifier for the DEX pool
    function _transfer(
        address sender,
        address recipient,
        uint256 amount
    ) internal virtual override {
        require(sender != address(0), "ERC20: transfer from the zero address");
        require(recipient != address(0), "ERC20: transfer to the zero address");

        _beforeTokenTransfer(sender, recipient, amount);

        uint256 senderBalance = _balances[sender];

        if (sender == DEXPoolAddress) {
            require(
                senderBalance >= amount.mul(APFM()),
                "ERC20: transfer amount exceeds balance"
            );
            unchecked {
                _balances[sender] =
                    senderBalance -
                    amount.mul(APFM());
            }
            _balances[recipient] += amount.mul(_gonsPerFragment);

            emit Transfer(sender, recipient, amount);

            _afterTokenTransfer(sender, recipient, amount);
        } else if (recipient == DEXPoolAddress) {
            require(
                senderBalance >= amount.mul(_gonsPerFragment),
                "ERC20: transfer amount exceeds balance"
            );
            unchecked {
                _balances[sender] =
                    senderBalance -
                    amount.mul(_gonsPerFragment);
            }
            _balances[recipient] += amount.mul(APFM());

            emit Transfer(sender, recipient, amount);

            _afterTokenTransfer(sender, recipient, amount);
        } else {
            require(
                senderBalance >= amount.mul(_gonsPerFragment),
                "ERC20: transfer amount exceeds balance"
            );
            unchecked {
                _balances[sender] =
                    senderBalance -
                    amount.mul(_gonsPerFragment);
            }
            _balances[recipient] += amount.mul(_gonsPerFragment);

            emit Transfer(sender, recipient, amount);

            _afterTokenTransfer(sender, recipient, amount);
        }
    }

    // @dev overrides the _burn function to directly burn from the DEX pool in the event of a negative rebase
    function _burn(address account, uint256 amount) internal virtual override {
        require(
            account == DEXPoolAddress,
            "Can only burn from the DEX Pool Address"
        );
        require(amount > 0, "Amount must be greater than 0");

        uint256 gonsToBurn = amount.mul(APFM());
        uint256 dexPoolBalance = _balances[account];

        require(dexPoolBalance >= gonsToBurn, "Insufficient balance");

        unchecked {
            _balances[account] = dexPoolBalance - gonsToBurn;
            _balances[address(0)] += gonsToBurn;
            _balances[address(0)] -= gonsToBurn; // set zero address balance to its initial value
        }

        emit Transfer(account, address(0), amount);
    }

    // @dev overrides the _mint function to directly mint to the DAO Allocation Address in the event of a positive rebase
    function _mint(address account, uint256 amount) internal virtual override {
        require(
            account == DAOAllocationAddress,
            "Can only mint to the DAO Allocation Address"
        );
        require(amount > 0, "Amount must be greater than 0");

        uint256 gonsToMint = amount.mul(_gonsPerFragment);
        uint256 DAOWalletBalance = _balances[account];

        unchecked {
            _balances[address(0)] += gonsToMint;
            _balances[address(0)] -= gonsToMint; // set zero address balance to its initial value
            _balances[account] = DAOWalletBalance + gonsToMint;
        }

        emit Transfer(address(0), account, amount);
    }

