    // Gain Stability Wallet address
    address public GSWallet;

    // Gain Liquidity Wallet address
    address public GLWallet;

    // Gain Voucher Wallet address
    address public GVWallet;

    // Gain stability tax fee
    uint256 GsFee;

    // Gain staking tax fee
    uint256 GecoFee;

    // Gain liquidity tax fee
    uint256 GlFee;

    // Tax enable disable status
    bool public sellStatus;

    // liquity staking contract address
    address public LiqStakingContract;

    /**
     * Contributed by MrKawdy:
     *
     * Gainv4_1
     *
     * Removed daiLiquidityReserves() from totalReserves() function to help ensure GAIN is truly fully backed by DAI reserves daily regardless of market fluctuations.
     * Modified InFS by increasing the range of accepted values to allow for setting InFS when total supply is less than it.
     * Improved readability across the contract by changing large numbers to use scientific notation instead of raw numbers.
     *
     * Gainv4
     *
     * Revamped the rebase mechanics to exclude the DEX pool from positive rebases and allocate the DEX pool's portion of each positive rebase to the DAO.
     *
     * This involved adding the following:
     *
     * Adaptive Pool Fragment Modifier (APFM): function that allows for excluding the DEX pool's balance from rebases.
     * balanceOf: overrode the balanceOf function to return the correct balance for the DEX pool + all other addresses.
     * getHolderRebase: function for retrieving an individual holder's portion of the next rebase.
     * getsupplydelta(): function for returning a continous calculation of the _supplyDelta.
     * Inception Fragment Supply (InFS): initial fixed value used as the denominator to calculate APFM().
     * DEXPoolAddress: address of the DEX pool.
     * DAOAllocationAddress: address that is sent the DAO Allocation of each positive rebase.
     * _transfer: overrode the _transfer function for proper transfers between the DEX pool and all other addresses.
     * _burn: overrode the _burn function to directly burn from the DEX pool in the event of a negative rebase.
     * _mint: overrode the _mint function to directly mint to the DAO Allocation Address in the event of a positive rebase.
     *
     * More additions/changes:
     *
     * Got rid of gainPriceInDai() and replaced it with the more precise gainDEXPrice() function.
     * Renamed CurrentRatio(), Target, and Stretch to Real-Time Reserve Ratio (RTRR), Target Reserve Ratio (TRR), and Dynamic Supply Modifier (DSM) 
     * respectively for more clarity.
     * Modified setTRR and setDSM to have an upper limit (5:1 max TRR and 100x max DSM) to strike a balance between flexibility and protection.
     *
     * Gainv3_5
     *
     * Removed the Stretch from negative rebases to guarantee (unless - a protocol hack or automation failure) that GAIN maintains >=100% backing every day.
     * Changed the feeCalculation function to use a divisor of 10000 instead of 1000 for the ability to set fees in 0.01% increments.
     *
     * Gainv3_1
     *
     * DEXFees: address for DEX Fees that can drastically overcharge GAIN and other Rebase tokens fees when a DEX has activated Uniswap V2 protocol fees.
     * This address is not allowed to transfer GAIN tokens due to the error.
     * @notice - DEX protocols are sent the correct fees on a monthly basis from END's treasury. Fees = total monthly trading volume * protocol fee.
     *
     * Gainv3
     *
     * Cleaned up rebase calculation to use powers of 10 instead of large numbers and round down total supply for viewing purposes.
     * Gain_Dai: renamed from Gain_Usd to Gain_Dai for clarification purposes
     * GRMultisig: address for Multisig containing GAIN's reserves including the DAI GAIN LP tokens + DAI Reserves.
     * daiReserves(): function for retrieving the DAI Reserves in the GAIN Reserves Multisig.
     * daiLiquidityReserves(): function for retrieving the DAI Liquidity Reserves paired with GAIN in the DEX liquidity pool.
     * totalReserves(): function for retrieving the sum of DAI Liquidity Reserves + DAI Reserves in the GAIN Reserves Multisig.
     *
     * @notice - The Current Ratio, Target, and Stretch are scaled up by a factor of 1000 for both viewing and calculation purposes.
     * Example - If the Current Ratio is publicly displayed as 2000, Target as 1100, and Stretch as 1000, this means that
     * Current Ratio = 2.000, Target = 1.100, and Stretch = 1.000.
     *
     * CurrentRatio(): function for retrieving the Total Reserves:Total Supply ratio.
     * Target: allows for GAIN to have the rebase target a backing of greater than 100%. This value always has to be >= 1000 in which 1000 = 100% backing.
     * Stretch: allows for softening of each rebase. For example, a stretch of 10000 = total supply +/- 1/10th the amount it would with a stretch = 1000.
     * DaiAddress: contract address of DAI.
     * RebaseWhitelist/Whitelisted: allows for whitelisting of addresses or contracts to execute the rebase for automation purposes.
     *
     * @notice - these additions allow GAIN to both transparently and automatically maintain a backing >= 100% as well as optionally soften rebases for stability.
     *
     */

    // GAIN Reserves Multisig
    address public GRMultisig;

    // GAIN DAI Reserves
    function daiReserves() public view virtual returns (uint256) {
        return IERC20Upgradeable(DaiAddress).balanceOf(GRMultisig);
    }

    // GAIN DAI Liquidity Reserves
    function daiLiquidityReserves() public view virtual returns (uint256) {
        uint112 _reserve0;
        uint112 _reserve1;
        uint112 _temp;

        (_reserve0, _reserve1, ) = Gain_Dai.getReserves();

        // interchange the token reserve according to token position.
        if (Gain_Dai.token1() == address(this)) {
            _temp = _reserve0;
            _reserve0 = _reserve1;
            _reserve1 = _temp;
        }

        return uint256(_reserve1);
    }

    // DEX Pool GAIN Balance
    function dexPoolGAIN() public view virtual returns (uint256) {
        uint112 _reserve0;
        uint112 _reserve1;
        uint112 _temp;

        (_reserve0, _reserve1, ) = Gain_Dai.getReserves();

        // interchange the token reserve according to token position.
        if (Gain_Dai.token1() == address(this)) {
            _temp = _reserve0;
            _reserve0 = _reserve1;
            _reserve1 = _temp;
        }

        return uint256(_reserve0);
    }

    // GAIN DEX Price
    function gainDEXPrice() public view virtual returns (uint256) {
        return daiLiquidityReserves().mul(10 ** 18).div(dexPoolGAIN());
    }

    // GAIN Total Reserves
    function totalReserves() public view virtual returns (uint256) {
        return daiReserves();
    }

    // GAIN Real-Time Reserve Ratio (RTRR)
    function RTRR() public view virtual returns (uint256) {
        return totalReserves().mul(1000).div(_totalSupply);
    }

    // GAIN Adaptive Pool Fragment Modifier
    function APFM() internal view virtual returns (uint256) {
        return TOTAL_GONS.div(InFS);
    }

    // @dev Overrides the balanceOf function by using the Adaptive Pool Fragment Modifier for the DEX pool.
    function balanceOf(
        address account
    ) public view virtual override returns (uint256) {
        if (account == DEXPoolAddress) {
            return _balances[account].div(APFM());
        } else {
            return _balances[account].div(_gonsPerFragment);
        }
    }

    // Get holder's rebase
    function getHolderRebase(address holder) public view returns (uint256) {
        uint256 holderBalance = balanceOf(holder);
        uint256 totalSupply = totalSupply();
        int256 supplyDelta = getsupplydelta();
        uint256 holderShare = holderBalance.mul(10 ** 18).div(totalSupply); // Calculate the holder's share of the total supply with 18 decimal places of precision
        uint256 holderRebase = uint256(supplyDelta.abs()).mul(holderShare).div(
            10 ** 18
        ); // Calculate the holder's share of the rebase with 18 decimal places of precision
        return holderRebase;
    }

    // Continuous Supply Delta Calculation
    function getsupplydelta() public view virtual returns (int256) {
        int256 _supplyDelta = 0;
        uint256 unroundedTotalSupply = totalSupply();
        uint256 roundedTotalSupply = 0;
        if (RTRR() > TRR) {
            uint256 reservePercentage = uint256(
                (totalReserves() * 10 ** 3) - (_totalSupply * TRR)
            );
            uint256 percentage = (reservePercentage * 10 ** 20) /
                uint256(_totalSupply * TRR);

            uint256 totalSupply = IERC20Upgradeable(address(this))
                .totalSupply();
            uint256 _data = percentage * totalSupply;

            _supplyDelta = int256(((_data * 10 ** 3) / DSM) / 10 ** 20);
            if (_supplyDelta != 0) {
                unroundedTotalSupply = unroundedTotalSupply.add(
                    uint256(_supplyDelta)
                );
                roundedTotalSupply =
                    (unroundedTotalSupply / 10 ** 18) *
                    10 ** 18;
                _supplyDelta = int256(roundedTotalSupply) - int256(totalSupply);
                return int256(_supplyDelta);
            }
        } else if (RTRR() < TRR) {
            uint256 reservePercentage = uint256(
                (_totalSupply * TRR) - (totalReserves() * 10 ** 3)
            );
            uint256 percentage = (reservePercentage * 10 ** 20) /
                uint256(_totalSupply * TRR);

            uint256 totalSupply = IERC20Upgradeable(address(this))
                .totalSupply();
            uint256 _data = percentage * totalSupply;

            _supplyDelta = -int256(_data / 10 ** 20);
            if (_supplyDelta != 0) {
                unroundedTotalSupply = unroundedTotalSupply.sub(
                    uint256(_supplyDelta.abs())
                );
                roundedTotalSupply =
                    (unroundedTotalSupply / 10 ** 18) *
                    10 ** 18;
                _supplyDelta = int256(totalSupply) - int256(roundedTotalSupply);
                return int256(_supplyDelta.abs());
            }
        }
        return int256(_supplyDelta.abs());
    }

    // GAIN Target Reserve Ratio (TRR)
    uint256 public TRR;

    // GAIN Dynamic Supply Modifier (DSM)
    uint256 public DSM;

    // GAIN Inception Fragment Supply (InFS)
    uint256 public InFS;

    // DAI Address
    address public DaiAddress;

    // DEX Router
    IUniswapV2Router01 public RouterAddress;

    // DEX Fees
    address public DEXFees;

    // DEX Pool Address
    address public DEXPoolAddress;

    // DAO Allocation Address
    address public DAOAllocationAddress;

    // Whitelist mapping For Rebasing Purposes
    bool public enableRebaseWhitelist = true;
    mapping(address => bool) public whitelisted;

    // Events
    event Whitelisted(address indexed _address);
    event WhitelistRemoved(address indexed _address);
    event Rebase(
        uint256 indexed epoch,
        uint256 totalSupply,
        int256 supplyDelta
    );

