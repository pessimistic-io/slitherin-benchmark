    require(_exists(_tokenId), "DogShitNFT::tokenURI:: token not existed");

    string memory _tokenURI = _tokenURIs[_tokenId];
    string memory _categoryURI = categoryInfo[dogeshitNFTToCategory[_tokenId]].categoryURI;

    // If if category uri exists, use categoryURI as a tokenURI
    if (bytes(_categoryURI).length > 0 && bytes(_tokenURI).length > 0) {
      return string(abi.encodePacked(categoryInfo[dogeshitNFTToCategory[_tokenId]].categoryURI, _tokenURI));
    }

    return "";
  }

