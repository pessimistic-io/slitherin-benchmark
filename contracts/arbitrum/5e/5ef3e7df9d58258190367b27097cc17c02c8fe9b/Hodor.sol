pragma solidity ^0.8.10;







contract Hodor is ERC20 {
    error Unauthorized();
    error VaultAlreadySet();
    error CanNotBuyAndSellSameTime();
    error FusingNow();
    error OutOfMint();

    IHodorController public controller;

    address public vault; // uHodor vault address
    address public lucky; // lucky drop
    address public luckyOperator; // operator lucky drop
    address public invitePool;

    uint256 public constant BURN_FEE = 845; // 0.845%
    uint256 public constant INVITE_FEE = 1155; // 1.155%
    uint256 public constant STAKER_FEE = 650; // 0.65%
    uint256 public constant LUCKY_FEE = 650; // 0.65%
    uint256 public constant MAX_SUPPLY = 58504624 * 1e18;

    uint256 internal constant PRECISION = 1e18;

    bool public isRebalancing;
    bool public fusing;
    bool public initialized;

    mapping(address => bool) public feeWhitelist;
    mapping(address => uint256) _buyerTime;
    mapping(address => uint256) _sellerTime;

    constructor() ERC20("Hold the door", "HODOR", 18) {}

    function initialization(
        address lucky_,
        address luckyOperator_,
        address invitePool_,
        address bank_
    )external {
        require(!initialized);
        // Mint initial supply to controller
        controller = IHodorController(msg.sender);
        lucky = lucky_;
        luckyOperator = luckyOperator_;
        invitePool = invitePool_;
        feeWhitelist[lucky] = true;
        feeWhitelist[invitePool] = true;
        feeWhitelist[msg.sender] = true;
        feeWhitelist[address(this)] = true;
        feeWhitelist[bank_] = true;

        initialized = true;

    }

    function mint(address to_, uint256 amount_) external {
        if (MAX_SUPPLY < (totalSupply + amount_)) revert OutOfMint();
        if (msg.sender != address(controller)) revert Unauthorized();
        _mint(to_, amount_);
    }

    function setVault(address vault_) external {
        if (msg.sender != address(controller)) revert Unauthorized();
        if (vault != address(0)) revert VaultAlreadySet();
        vault = vault_;
        feeWhitelist[vault] = true;
    }

    function setIsRebalancing(bool isRebalancing_) external {
        if (msg.sender != address(controller)) revert Unauthorized();
        isRebalancing = isRebalancing_;
    }

    /// -----------------------------------------------------------------------
    /// OVERRIDES
    /// -----------------------------------------------------------------------
    function transfer(
        address to_,
        uint256 amount_
    ) public virtual override returns (bool) {
        balanceOf[msg.sender] -= amount_;

        uint256 _amount = _chargeTax(msg.sender, to_, amount_);

        unchecked {
            balanceOf[to_] += _amount;
        }

        if (!_isContract(msg.sender) && !isRebalancing) {
            controller.rebalance();
        }

        emit Transfer(msg.sender, to_, _amount);

        return true;
    }

    function transferFrom(
        address from_,
        address to_,
        uint256 amount_
    ) public virtual override returns (bool) {
        // Saves gas for limited approvals.
        uint256 allowed = allowance[from_][msg.sender];

        if (allowed != type(uint256).max)
            allowance[from_][msg.sender] = allowed - amount_;

        balanceOf[from_] -= amount_;

        uint256 _amount = _chargeTax(msg.sender, to_, amount_);

        unchecked {
            balanceOf[to_] += _amount;
        }

        if (!_isContract(msg.sender) && !isRebalancing) {
            controller.rebalance();
        }

        emit Transfer(from_, to_, _amount);

        return true;
    }

    /// -----------------------------------------------------------------------
    /// TAX LOGIC
    /// -----------------------------------------------------------------------

    function _chargeTax(
        address from,
        address to,
        uint256 amount
    ) internal returns (uint256 _amount) {
        _amount = amount;

        if (!isRebalancing) {
            if (!feeWhitelist[from] && !feeWhitelist[to]) {
                uint256 luckyFee = _calculateFee(_amount, LUCKY_FEE);
                uint256 inviteFee = _calculateFee(_amount, INVITE_FEE);
                uint256 burn = _calculateFee(_amount, BURN_FEE);
                uint256 sendToVault = _calculateFee(_amount, STAKER_FEE);
                balanceOf[lucky] += luckyFee;

                emit Transfer(from, lucky, luckyFee);
                balanceOf[invitePool] += inviteFee;
                emit Transfer(from, invitePool, inviteFee);

                balanceOf[vault] += sendToVault;
                emit Transfer(from, vault, sendToVault);

                unchecked {
                    totalSupply -= burn;
                }
                emit Transfer(from, address(0), burn);

                _amount -= (luckyFee + inviteFee + sendToVault + burn);

                // Call relevant rebalance functions on controller.
                // These will return early if not able to be called.
                // Buy tax
                if (_isSwap(from)) {
                    if (address(controller) != address(0) && controller.canRebalance()){
                        if (fusing) {
                            revert FusingNow();
                        }
                    }
                    if (block.timestamp == _sellerTime[to]) {
                        revert CanNotBuyAndSellSameTime();
                    }
                    if (lucky != address(0)) {
                        IHodorLuckyDrop luckyContract = IHodorLuckyDrop(lucky);
                        luckyContract.onTransfer(to, _amount);
                    }
                    _buyerTime[to] = block.timestamp;

                    if (_isJoePool(from)) {
                        ILBPair pair = ILBPair(from);
                        (uint256 x, ) =  pair.getBin(controller.maxBin());
                        if(x == 0 && controller.canRebalance()){
                            fusing = true;
                        }
                    }
                }

                if (_isSwap(to)) {
                    if (block.timestamp == _buyerTime[to]) {
                        revert CanNotBuyAndSellSameTime();
                    }
                    _sellerTime[from] = block.timestamp;
                    controller.shift();
                }
            }
        }
    }

    /// -----------------------------------------------------------------------
    /// MORE HELPERS AND VIEW FUNCS
    /// -----------------------------------------------------------------------

    function _calculateFee(
        uint256 amount,
        uint256 pct
    ) internal pure returns (uint256) {
        uint256 feePercentage = (PRECISION * pct) / 100000; // x pct
        return (amount * feePercentage) / PRECISION;
    }

    function _isJoePool(address target) internal view returns (bool) {
        if (target.code.length == 0) return false;

        ILBPair pool = ILBPair(target);

        try pool.getTokenX() {} catch (bytes memory) {
            return false;
        }

        try pool.getTokenY() {} catch (bytes memory) {
            return false;
        }

        try pool.getBinStep() {} catch (bytes memory) {
            return false;
        }

        return true;
    }

    function _isUniV3Pool(address target) internal view returns (bool) {
        if (target.code.length == 0) return false;

        IUniswapV3Pool pool = IUniswapV3Pool(target);

        try pool.token0() {} catch (bytes memory) {
            return false;
        }

        try pool.token1() {} catch (bytes memory) {
            return false;
        }

        try pool.fee() {} catch (bytes memory) {
            return false;
        }

        return true;
    }

    function _isUniV2Pair(address target) internal view returns (bool) {
        if (target.code.length == 0) return false;

        IUniswapV2Pair uniPair = IUniswapV2Pair(target);

        try uniPair.token0() {} catch (bytes memory) {
            return false;
        }

        try uniPair.token1() {} catch (bytes memory) {
            return false;
        }

        try uniPair.kLast() {} catch (bytes memory) {
            return false;
        }

        return true;
    }

    function _isSwap(address target) internal view returns (bool) {
        if (
            _isJoePool(target) || _isUniV2Pair(target) || _isUniV3Pool(target)
        ) {
            return true;
        }
        return false;
    }

    function _isContract(address account) internal view returns (bool) {
        return account.code.length > 0;
    }

    function setLucky(address lucky_) external {
        require(msg.sender == luckyOperator);
        lucky = lucky_;
    }

    function setLuckyOperator(address luckyOperator_) external {
        require(msg.sender == luckyOperator);
        luckyOperator = luckyOperator_;
    }

    function setFeeWhitelist(address addr_, bool isWhitelist_) external {
        require(
            msg.sender == address(controller),
            "No permission."
        );
        feeWhitelist[addr_] = isWhitelist_;
    }

    function setFusing(bool isFusing_) external {
        require(
            msg.sender == address(controller),
            "No permission."
        );
        fusing = isFusing_;
    }
}