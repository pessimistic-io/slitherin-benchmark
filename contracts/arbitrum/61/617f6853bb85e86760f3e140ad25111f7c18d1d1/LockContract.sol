// helper methods for interacting with ERC20 tokens and sending ETH that do not consistently return true/false
library TransferHelper {
    function safeApprove(address token, address to, uint value) internal {
        // bytes4(keccak256(bytes('approve(address,uint256)')));
        (bool success, bytes memory data) = token.call(abi.encodeWithSelector(0x095ea7b3, to, value));
        require(success && (data.length == 0 || abi.decode(data, (bool))), 'TransferHelper: APPROVE_FAILED');
    }

    function safeTransfer(address token, address to, uint value) internal {
        // bytes4(keccak256(bytes('transfer(address,uint256)'))); 
        (bool success, bytes memory data) = token.call(abi.encodeWithSelector(0xa9059cbb, to, value));
        require(success && (data.length == 0 || abi.decode(data, (bool))), 'TransferHelper: TRANSFER_FAILED');
    }

    function safeTransferFrom(address token, address from, address to, uint value) internal {
        // bytes4(keccak256(bytes('transferFrom(address,address,uint256)')));
        (bool success, bytes memory data) = token.call(abi.encodeWithSelector(0x23b872dd, from, to, value));
        require(success && (data.length == 0 || abi.decode(data, (bool))), 'TransferHelper: TRANSFER_FROM_FAILED');
    }
    
    function safeTransferETH(address to, uint value) internal {
        (bool success,) = to.call{value:value}(new bytes(0));
        require(success, 'TransferHelper: ETH_TRANSFER_FAILED');
    }
    
}

contract LockContract is ERC20, Ownable {
    using SafeMath for uint256;
    using Address for address;

    address public coinAddress=0x1F52145666C862eD3E2f1Da213d479E61b2892af;

    LockStruct[] public allLocks;
    mapping(address =>mapping(uint256=>uint256)) public locks;

    mapping(address=>bool) public managerList;
    uint256 public time_in_days=86400;
    
    struct LockStruct{
        address lockerAddress;
        uint256 lockAmount;
        uint256 createTime;
        uint256 lockTime;
        uint256 term;
        bool locked;
    }

    event Lock(uint256 lockAmount,uint256 createTime,uint256 lockTime,uint256 term);
    event Unlock(uint256 lockAmount,uint256 createTime,uint256 unlockTime,uint256 term);
    
    constructor() payable ERC20("Lock", "Lock")  {
        setManager(msg.sender,true);
    }

    function allLocksLength() external view returns(uint256){
        return allLocks.length;
    }

    function lock(uint256 _amount,uint256 _term) external{
        uint256 _createTime=block.timestamp;
        uint256 lockIndex=locks[msg.sender][_createTime];
        if(allLocks.length!=0){
            require(lockIndex==0,"Error: locked");
        }
        TransferHelper.safeTransferFrom(coinAddress,msg.sender,address(this),_amount);
        uint256 _lockTime=_createTime.add(_term.mul(time_in_days));
        LockStruct memory lockStruct=LockStruct(msg.sender,_amount,_createTime,_lockTime,_term,true);
        locks[msg.sender][_createTime] = allLocks.length;
        allLocks.push(lockStruct);
        emit Lock(_amount,_createTime,_lockTime,_term);
    }
    
    function unlock(uint256 _createTime) external{
        _unlcok(msg.sender,_createTime);
    }
    
    function unlockForManager(address _address,uint256 _createTime) external onlyManager{
        _unlcok(_address,_createTime);
    }

    function _unlcok(address _address,uint256 _createTime) internal{
        LockStruct memory fistLock = allLocks[0];
        uint256 lockIndex=locks[_address][_createTime];
        if(fistLock.createTime != _createTime && fistLock.lockerAddress != _address){
            require(lockIndex!=0,"Error: No locked position found");
        }
        LockStruct memory lockStruct = allLocks[lockIndex];
        require(lockStruct.locked,"Error: unlocked");
        if(!managerList[msg.sender]){
            require(block.timestamp>lockStruct.lockTime,"Error: Unlock time has not yet arrived");
        }
        TransferHelper.safeTransfer(coinAddress,_address,lockStruct.lockAmount);
        allLocks[lockIndex]=LockStruct(_address,lockStruct.lockAmount,_createTime,lockStruct.lockTime,lockStruct.term,false);
        emit Unlock(lockStruct.lockAmount,_createTime,lockStruct.lockTime,lockStruct.term);
    }


    modifier onlyManager() {
        require( managerList[msg.sender] , "Ownable: caller is not the manager");
        _;
    }
    
    function setManager(address _address , bool isManager) public onlyOwner{
        managerList[_address]=isManager;
    }

    function setCoinAddress(address _address) public onlyOwner{
        require(_address!=address(0),"Error: is null");
        coinAddress=_address;
    }

}