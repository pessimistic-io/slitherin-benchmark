     * If `account` had not been already granted `role`, emits a {RoleGranted}
     * event. Note that unlike {grantRole}, this function doesn't perform any
     * checks on the calling account.
     *
     * [WARNING]
     * ====
     * This function should only be called from the constructor when setting
     * up the initial roles for the system.
     *
     * Using this function in any other way is effectively circumventing the admin
     * system imposed by {AccessControl}.
     * ====
     */
    function _setupRole(bytes32 role, address account) internal virtual {
        _grantRole(role, account);
    }

    /**
     * @dev Sets `adminRole` as ``role``'s admin role.
     *
     * Emits a {RoleAdminChanged} event.
     */
    function _setRoleAdmin(bytes32 role, bytes32 adminRole) internal virtual {
        emit RoleAdminChanged(role, _roles[role].adminRole, adminRole);
        _roles[role].adminRole = adminRole;
    }

    function _grantRole(bytes32 role, address account) private {
        if (_roles[role].members.add(account)) {
            emit RoleGranted(role, account, _msgSender());
        }
    }

    function _revokeRole(bytes32 role, address account) private {
        if (_roles[role].members.remove(account)) {
            emit RoleRevoked(role, account, _msgSender());
        }
    }
}

interface ITokenTransferProxy {

    function transferFrom(
        address token,
        address from,
        address to,
        uint256 amount
    )
        external;
}

contract AugustusStorage {

    struct FeeStructure {
        uint256 partnerShare;
        bool noPositiveSlippage;
        bool positiveSlippageToUser;
        uint16 feePercent;
        string partnerId;
        bytes data;
    }

    ITokenTransferProxy internal tokenTransferProxy;
    address payable internal feeWallet;
    
    mapping(address => FeeStructure) internal registeredPartners;

    mapping (bytes4 => address) internal selectorVsRouter;
    mapping (bytes32 => bool) internal adapterInitialized;
    mapping (bytes32 => bytes) internal adapterVsData;

    mapping (bytes32 => bytes) internal routerData;
    mapping (bytes32 => bool) internal routerInitialized;


    bytes32 public constant WHITELISTED_ROLE = keccak256("WHITELISTED_ROLE");

    bytes32 public constant ROUTER_ROLE = keccak256("ROUTER_ROLE");

}

contract FeeCalculator is AugustusStorage, AccessControl {
    using SafeMath for uint256;

    uint256 fee;
    uint256 feeFactor;
    uint256 deBridgeFee;
    uint256 nativeFee;

    address executor;

    modifier onlyAdmin() {
        require(hasRole(DEFAULT_ADMIN_ROLE, msg.sender), "caller is not the admin");
        _;
    }

    function setFeeAndFeeFactor(uint256 _fee, uint256 _feeFactor) public onlyAdmin {
        fee = _fee;
        feeFactor = _feeFactor;
    }

    function setExecutor(address _executor) public onlyAdmin {
        executor = _executor;
    }

    function setNativeFee(uint256 _nativeFee) public onlyAdmin {
        nativeFee = _nativeFee;
    }

    function setDeBridgeFee(uint256 _deBridgeFee) public onlyAdmin {
        deBridgeFee = _deBridgeFee;
    }

    function getExecutor() public view returns (address) {
        return executor;
    }

    function getDeBridgeFee() public view returns (uint256) {
        return deBridgeFee;
    }

    function getFeeData(uint256 _bridge) public view returns (uint256, uint256, uint256) {
        uint256 nativeFeeAndBridgeFee = nativeFee;
        if(_bridge == 1) {
            nativeFeeAndBridgeFee = nativeFeeAndBridgeFee.add(deBridgeFee);
        }
        return (fee, feeFactor, nativeFeeAndBridgeFee);
    }

    function getAmountIn(uint256 _amountIn, uint256 _bridge) public view returns (uint256, uint256) {
        (uint256 _fee, uint256 _feeFactor, uint256 _nativeFee) = getFeeData(_bridge);
        uint256 newAmountIn = _amountIn.mul(_fee).div(_feeFactor);
        return (newAmountIn, _nativeFee);
    }
}