abstract contract Stakeable is Ownable, IArrayStaking, WithLevels {
    using SafeMath for uint256;
    using SafeERC20 for IERC20;
    using EnumerableSet for EnumerableSet.AddressSet;

    // Info of persons who have special role
    struct Person {
        address addr;
        uint256 share; //100 = 0.1%
    }

    uint256 public stakerAllocation; //700 = 0.7%

    // Info of each user.
    struct UserInfo {
        uint256 amount; // How many tokens the user has provided.
        uint256 rewardDebt; // Reward debt. See explanation below.
    }

    uint256 public rewardPerBlock; // 1 * 1e18 = 1 token
    Array public rewardToken;

    uint256 public lockPeriod;

    struct Fees {
        address collectorAddress;
        // base 1000, 20% = 200
        uint256 depositFee;
        uint256 withdrawFee;
        uint256 collectedDepositFees;
        uint256 collectedWithdrawFees;
    }

    // Info of  pool.
    struct PoolInfo {
        IERC20 stakingToken; // Token to stake
        uint256 lastRewardBlock; // Last block number that array distribution occurs.
        uint256 accArrayPerShare; // Accumulated array per share, times 1e12. See below.
    }

    Fees public fees;

    // founder info
    Person public founder;

    // developer info
    Person public developer;

    // marketing info
    Person public marketing;

    // advisor info
    Person public advisor;

    // Info of  pool.
    PoolInfo public poolInfo;

    // Keeps reward tokens
    StakingTreasury public treasury;

    // Info of each user that stakes token tokens.
    mapping(address => UserInfo) public userInfo;

    EnumerableSet.AddressSet private poolUsers;

    // The date of user staking stake time.
    mapping(address => uint256) public depositLockStart;

    // events which will emit on different tasks
    event Deposit(address indexed user, uint256 amount);
    event Withdraw(address indexed user, uint256 amount);
    event EmergencyWithdraw(address indexed user, uint256 amount);
    event FeesUpdated(uint256 depositFee, uint256 withdrawFee);

    constructor(
        Person memory _founder,
        Person memory _developer,
        Person memory _marketing,
        Person memory _advisor,
        uint256 _stakerAllocation,
        uint256 _lockPeriod
    ) {
        founder = _founder;
        developer = _developer;
        marketing = _marketing;
        advisor = _advisor;
        stakerAllocation = _stakerAllocation;
        require(
            founder.share +
                developer.share +
                marketing.share +
                advisor.share +
                stakerAllocation ==
                1000,
            "Total allocation should be 100"
        );
        lockPeriod = _lockPeriod;

        treasury = new StakingTreasury();
    }

    modifier lockable() {
        require(!isLocked(_msgSender()), "Account is locked");
        _;
    }

    // Add a new pool. Can only be called by the owner.
    // XXX DO NOT add the same  token more than once. Rewards will be messed up if you do.
    function setPool(
        IERC20 _stakingToken,
        Array _rewardToken,
        uint256 _rewardPerBlock
    ) public onlyOwner {
        require(
            address(rewardToken) == address(0) &&
                address(poolInfo.stakingToken) == address(0),
            "Token is already set"
        );
        poolInfo = PoolInfo(_stakingToken, 0, 0);
        rewardToken = _rewardToken;
        rewardPerBlock = _rewardPerBlock;
        treasury.allowClaiming(_rewardToken);
    }

    // Return reward multiplier over the given _from to _to block.
    function getMultiplier(uint256 _from, uint256 _to)
        public
        pure
        returns (uint256)
    {
        return _to.sub(_from);
    }

    // Update reward variables of the  pool to be up-to-date.
    function updatePool() public {
        PoolInfo storage pool = poolInfo;
        if (block.number <= pool.lastRewardBlock) {
            return;
        }
        uint256 tokenSupply = poolInfo.stakingToken.balanceOf(address(this));
        if (tokenSupply == 0) {
            pool.lastRewardBlock = block.number;
            return;
        }
        uint256 multiplier = getMultiplier(pool.lastRewardBlock, block.number);
        uint256 reward = multiplier.mul(rewardPerBlock);
        rewardToken.mint(founder.addr, reward.mul(founder.share).div(1000));
        rewardToken.mint(advisor.addr, reward.mul(advisor.share).div(1000));
        rewardToken.mint(developer.addr, reward.mul(developer.share).div(1000));
        rewardToken.mint(marketing.addr, reward.mul(marketing.share).div(1000));
        uint256 arrayReward = reward.mul(stakerAllocation).div(1000);
        rewardToken.mint(address(treasury), arrayReward);
        poolInfo.accArrayPerShare = poolInfo.accArrayPerShare.add(
            arrayReward.mul(1e12).div(tokenSupply)
        );
        pool.lastRewardBlock = block.number;
    }

    function isLocked(address account) public view override returns (bool) {
        return block.timestamp < depositLockStart[account] + lockPeriod;
    }

    function reDeposit() public virtual {
        require(
            poolInfo.stakingToken == rewardToken,
            "Can't redeposit  tokens differ"
        );
        address account = msg.sender;
        UserInfo storage user = userInfo[account];
        updatePool();
        if (user.amount > 0) {
            uint256 pending = user
                .amount
                .mul(poolInfo.accArrayPerShare)
                .div(1e12)
                .sub(user.rewardDebt);
            safeRewardTransfer(address(this), pending);
            Tier memory prevTier = getTierForAmount(user.amount);
            user.amount = user.amount.add(pending);
            user.rewardDebt = user.amount.mul(poolInfo.accArrayPerShare).div(
                1e12
            );
            Tier memory cTier = getTierForAmount(user.amount);
            if (!isLocked(account) || !stringsEqual(prevTier.id, cTier.id)) {
                depositLockStart[account] = block.timestamp;
            }
        }
    }

    // Deposit  tokens  for array allocation.
    function deposit(uint256 _amount) public virtual {
        address account = msg.sender;
        UserInfo storage user = userInfo[account];
        uint256 fee;
        updatePool();
        poolInfo.stakingToken.safeTransferFrom(
            address(account),
            address(this),
            _amount
        );
        (_amount, fee) = takeFee(_amount, fees.depositFee);
        fees.collectedDepositFees = fees.collectedDepositFees.add(fee);
        if (user.amount > 0) {
            uint256 pending = user
                .amount
                .mul(poolInfo.accArrayPerShare)
                .div(1e12)
                .sub(user.rewardDebt);
            safeRewardTransfer(account, pending);
        }
        Tier memory prevTier = getTierForAmount(user.amount);
        user.amount = user.amount.add(_amount);
        user.rewardDebt = user.amount.mul(poolInfo.accArrayPerShare).div(1e12);
        Tier memory cTier = getTierForAmount(user.amount);
        if (!isLocked(account) || !stringsEqual(prevTier.id, cTier.id)) {
            depositLockStart[account] = block.timestamp;
        }
        if (user.amount > 0) {
            poolUsers.add(account);
        }
        emit Deposit(account, _amount);
    }

    // View function to see pending Array on frontend.
    function pendingArray(address _user) external view returns (uint256) {
        UserInfo storage user = userInfo[_user];
        uint256 accArrayPerShare = poolInfo.accArrayPerShare;
        uint256 supply = poolInfo.stakingToken.balanceOf(address(this));
        if (block.number > poolInfo.lastRewardBlock && supply != 0) {
            uint256 multiplier = getMultiplier(
                poolInfo.lastRewardBlock,
                block.number
            );
            uint256 arrayReward = multiplier
                .mul(rewardPerBlock)
                .mul(stakerAllocation)
                .div(1000);
            accArrayPerShare = accArrayPerShare.add(
                arrayReward.mul(1e12).div(supply)
            );
        }
        return user.amount.mul(accArrayPerShare).div(1e12).sub(user.rewardDebt);
    }

    // Withdraw  tokens from  user
    function withdraw(uint256 _amount) public virtual {
        address account = msg.sender;
        UserInfo storage user = userInfo[account];
        bool tokensLocked = isLocked(account);
        uint256 fee;
        require(!tokensLocked, "Tokens are locked");
        require(user.amount >= _amount, "withdraw: not good");
        updatePool();
        uint256 pending = user
            .amount
            .mul(poolInfo.accArrayPerShare)
            .div(1e12)
            .sub(user.rewardDebt);
        safeRewardTransfer(account, pending);
        user.amount = user.amount.sub(_amount);
        user.rewardDebt = user.amount.mul(poolInfo.accArrayPerShare).div(1e12);
        (_amount, fee) = takeFee(_amount, fees.withdrawFee);
        fees.collectedWithdrawFees = fees.collectedWithdrawFees.add(fee);
        poolInfo.stakingToken.safeTransfer(address(account), _amount);
        if (user.amount <= 0) {
            poolUsers.remove(account);
        }
        emit Withdraw(account, _amount);
    }

    function takeFee(uint256 amount, uint256 feePercent)
        internal
        returns (uint256, uint256)
    {
        if (feePercent == 0) {
            return (amount, 0);
        }

        uint256 feeAmount = amount.mul(feePercent).div(1000);
        poolInfo.stakingToken.safeTransfer(fees.collectorAddress, feeAmount);

        return (amount.sub(feeAmount), feeAmount);
    }

    // Withdraw without caring about rewards. EMERGENCY ONLY.
    function emergencyWithdraw() public {
        UserInfo storage user = userInfo[msg.sender];
        poolInfo.stakingToken.safeTransfer(address(msg.sender), user.amount);
        emit EmergencyWithdraw(msg.sender, user.amount);
        user.amount = 0;
        user.rewardDebt = 0;
    }

    function safeRewardTransfer(address to, uint256 amount)
        internal
        returns (uint256)
    {
        uint256 balance = poolInfo.stakingToken.balanceOf(address(treasury));
        require(amount > 0, "Reward amount must be more than zero");
        require(balance > 0, "Not enough reward tokens for transfer");
        if (amount > balance) {
            poolInfo.stakingToken.safeTransferFrom(
                address(treasury),
                to,
                balance
            );
            return balance;
        }
        poolInfo.stakingToken.safeTransferFrom(address(treasury), to, amount);
        return amount;
    }

    function updateRewardPerBlock(uint256 _rewardPerBlock) external onlyOwner {
        rewardPerBlock = _rewardPerBlock;
    }

    function updateLockPeriod(uint256 _lockPeriod) external onlyOwner {
        lockPeriod = _lockPeriod;
    }

    function setFees(
        address _feeAddress,
        uint256 _depositFee,
        uint256 _withdrawFee
    ) public onlyOwner {
        require(
            _feeAddress != address(0),
            "Fees collector address is not specified"
        );
        require(_depositFee < 700, "Max deposit fee: 70%");
        require(_withdrawFee < 700, "Max withdraw fee: 70%");

        fees.collectorAddress = _feeAddress;
        fees.depositFee = _depositFee;
        fees.withdrawFee = _withdrawFee;
        emit FeesUpdated(_depositFee, _withdrawFee);
    }

    modifier validShare(uint256 _share, uint256 _oldShare) {
        require(
            founder.share +
                developer.share +
                marketing.share +
                advisor.share +
                stakerAllocation +
                _share -
                _oldShare ==
                1000,
            "Invalid percentage"
        );
        _;
    }

    function setDeveloper(address _addr, uint256 _share)
        public
        validShare(_share, developer.share)
        onlyOwner
    {
        require(_addr != address(0), "address should't be 0");
        developer = Person({addr: _addr, share: _share});
    }

    function setMarketing(address _addr, uint256 _share)
        public
        validShare(_share, marketing.share)
        onlyOwner
    {
        require(_addr != address(0), "address should't be 0");
        marketing = Person({addr: _addr, share: _share});
    }

    function setAdvisor(address _addr, uint256 _share)
        public
        validShare(_share, advisor.share)
        onlyOwner
    {
        require(_addr != address(0), "address should't be 0");
        advisor = Person({addr: _addr, share: _share});
    }

    function setFounder(address _addr, uint256 _share)
        public
        validShare(_share, founder.share)
        onlyOwner
    {
        require(_addr != address(0), "address should't be 0");
        founder = Person({addr: _addr, share: _share});
    }

    function getLockedAmount(address account)
        external
        view
        override
        returns (uint256)
    {
        return userInfo[account].amount;
    }

    function getUserUnlockTime(address account)
        public
        view
        override
        returns (uint256)
    {
        return depositLockStart[account] + lockPeriod;
    }

    function getUserTier(address account)
        external
        view
        override
        returns (Tier memory)
    {
        UserInfo memory user = userInfo[account];
        return getTierForAmount(user.amount);
    }

    function getAllPoolUserInfos()
        public
        view
        returns (
            address[] memory,
            uint256[] memory,
            Tier[] memory
        )
    {
        address[] memory userAddresses = new address[](poolUsers.length());
        uint256[] memory amounts = new uint256[](poolUsers.length());
        Tier[] memory tiers = new Tier[](poolUsers.length());
        for (uint256 i = 0; i < poolUsers.length(); i++) {
            userAddresses[i] = poolUsers.at(i);
            amounts[i] = userInfo[poolUsers.at(i)].amount;
            tiers[i] = getTierForAmount(amounts[i]);
        }
        return (userAddresses, amounts, tiers);
    }
}

// File contracts/Staking/ArrayStaking.sol

pragma solidity =0.8.4;

contract ArrayStaking is Ownable, Stakeable {
    using SafeERC20 for IERC20;

    bool public halted = false;

    event Halted(bool status);

    constructor(
        Person memory _founder,
        Person memory _developer,
        Person memory _marketing,
        Person memory _advisor,
        uint256 _stakerAllocation,
        uint256 _lockPeriod
    )
        Stakeable(
            _founder,
            _developer,
            _marketing,
            _advisor,
            _stakerAllocation,
            _lockPeriod
        )
    {}

    function halt(bool status) external onlyOwner {
        halted = status;
        emit Halted(status);
    }

    modifier notHalted() {
        require(!halted, "Deposits are paused");
        _;
    }

    function deposit(uint256 amount) public override notHalted {
        super.deposit(amount);
    }

    function reDeposit() public override notHalted {
        super.reDeposit();
    }

    function withdraw(uint256 amount) public override lockable {
        super.withdraw(amount);
    }

    function withdrawRewards() external onlyOwner {
        treasury.withdrawRewards(rewardToken, _msgSender());
    }
}