contract ARC20 is ERC20 {
    uint256 public _maxSupply;
    uint256 public _amountPerMint;
    uint256 public _totalMinted;
    address public _factory;

    constructor(
        string memory tick,
        uint256 maxSupply,
        uint256 amountPerMint,
        address factory
    ) ERC20("ARC-20 Token", tick) {
        _maxSupply = maxSupply * 1e18;
