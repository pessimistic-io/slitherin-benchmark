                count++;
				if (parser.toksuper != -1)
					tokens[uint(parser.toksuper)].size++;
                continue;
            }

            // ' ', \r, \t, \n
            if (c == ' ' || c == 0x11 || c == 0x12 || c == 0x14) {
                continue;
            }

            // 0x3a
            if (c == ':') {
                parser.toksuper = int(parser.toknext -1);
                continue;
            }

            if (c == ',') {
                if (parser.toksuper != -1
                    && tokens[uint(parser.toksuper)].jsmnType != JsmnType.ARRAY
                    && tokens[uint(parser.toksuper)].jsmnType != JsmnType.OBJECT) {
                        for(i = parser.toknext-1; i>=0; i--) {
                            if (tokens[i].jsmnType == JsmnType.ARRAY || tokens[i].jsmnType == JsmnType.OBJECT) {
                                if (tokens[i].startSet && !tokens[i].endSet) {
                                    parser.toksuper = int(i);
                                    break;
                                }
                            }
                        }
                    }
                continue;
            }

            // Primitive
            if ((c >= '0' && c <= '9') || c == '-' || c == 'f' || c == 't' || c == 'n') {
                if (parser.toksuper != -1) {
                    token = tokens[uint(parser.toksuper)];
                    if (token.jsmnType == JsmnType.OBJECT
                        || (token.jsmnType == JsmnType.STRING && token.size != 0)) {
                            return (RETURN_ERROR_INVALID_JSON, tokens, 0);
                        }
                }

                r = parsePrimitive(parser, tokens, s);
                if (r != RETURN_SUCCESS) {
                    return (r, tokens, 0);
                }
                count++;
                if (parser.toksuper != -1) {
                    tokens[uint(parser.toksuper)].size++;
                }
                continue;
            }

            // printable char
            if (c >= 0x20 && c <= 0x7e) {
                return (RETURN_ERROR_INVALID_JSON, tokens, 0);
            }
        }

        return (RETURN_SUCCESS, tokens, parser.toknext);
    }

    function getBytes(string memory json, uint start, uint end) internal pure returns (string memory) {
        bytes memory s = bytes(json);
        bytes memory result = new bytes(end-start);
        for (uint i=start; i<end; i++) {
            result[i-start] = s[i];
        }
        return string(result);
    }

    // parseInt
    function parseInt(string memory _a) internal pure returns (int) {
        return parseInt(_a, 0);
    }

    // parseInt(parseFloat*10^_b)
    function parseInt(string memory _a, uint _b) internal pure returns (int) {
        bytes memory bresult = bytes(_a);
        int mint = 0;
        bool decimals = false;
        bool negative = false;
        for (uint i=0; i<bresult.length; i++){
            if ((i == 0) && (bresult[i] == '-')) {
                negative = true;
            }
            if ((uint8(bresult[i]) >= 48) && (uint8(bresult[i]) <= 57)) {
                if (decimals){
                   if (_b == 0) break;
                    else _b--;
                }
                mint *= 10;
                mint += int256(uint256(uint8(bresult[i]) - 48));
            } else if (uint8(bresult[i]) == 46) decimals = true;
        }
        if (_b > 0) mint *= int(10**_b);
        if (negative) mint *= -1;
        return mint;
    }

    function uint2str(uint i) internal pure returns (string memory){
        if (i == 0) return "0";
        uint j = i;
        uint len;
        while (j != 0){
            len++;
            j /= 10;
        }
        bytes memory bstr = new bytes(len);
        uint k = len - 1;
        while (i != 0){
            bstr[k--] = bytes1(uint8(48 + i % 10));
            i /= 10;
        }
        return string(bstr);
    }

    function parseBool(string memory _a) internal pure returns (bool) {
        if (strCompare(_a, 'true') == 0) {
            return true;
        } else {
            return false;
        }
    }

    function strCompare(string memory _a, string memory _b) internal pure returns (int) {
        bytes memory a = bytes(_a);
        bytes memory b = bytes(_b);
        uint minLength = a.length;
        if (b.length < minLength) minLength = b.length;
        for (uint i = 0; i < minLength; i ++)
            if (a[i] < b[i])
                return -1;
            else if (a[i] > b[i])
                return 1;
        if (a.length < b.length)
            return -1;
        else if (a.length > b.length)
            return 1;
        else
            return 0;
    }

}


// File contracts/Arbscriptions.sol


pragma solidity ^0.8.0;






contract Arbscriptions is ERC721, Ownable {
    using Counters for Counters.Counter;

    string public constant ARBS_HEAER  = "arb";
    bytes32 public constant ARBS_HEAER_HASH = keccak256(abi.encodePacked(ARBS_HEAER));
    uint8 public constant ARBS_VER = 1;
    uint256 public constant ARBS_ARBSCRIBE_FEE = 0.0003 ether;

    bytes32 public constant CT_TEXT_HASH = keccak256(abi.encodePacked("text/plain;charset=utf-8"));
    bytes32 public constant CT_JSON_HASH = keccak256(abi.encodePacked("application/json"));

    uint256 public constant ARC20_MAX_JSON_LENGTH = 100;
    uint256 public constant ARC20_MAX_JSON_TOKEN = 10;
    bytes32 public constant ARC20_P_HASH = keccak256(abi.encodePacked("p"));
    bytes32 public constant ARC20_OP_HASH = keccak256(abi.encodePacked("op"));
    bytes32 public constant ARC20_TICK_HASH = keccak256(abi.encodePacked("tick"));
    bytes32 public constant ARC20_MAX_HASH = keccak256(abi.encodePacked("max"));
    bytes32 public constant ARC20_LIM_HASH = keccak256(abi.encodePacked("lim"));
    bytes32 public constant ARC20_AMT_HASH = keccak256(abi.encodePacked("amt"));
    bytes32 public constant ARC20_P_HASH_ARC20 = keccak256(abi.encodePacked("arc-20"));
    bytes32 public constant ARC20_OP_HASH_DEPLOY = keccak256(abi.encodePacked("deploy"));
    bytes32 public constant ARC20_OP_HASH_MINT = keccak256(abi.encodePacked("mint"));

    address public Dev;
    string private baseURL;
    Counters.Counter private _inscriptionIdTracker;

    mapping(string => address) private arc20Contracts;
    mapping(address => uint256) private arc20TokenIndex;
    
    struct ARC20Token {
        address tokenAddress;
        string tick;
        uint256 maxSupply;
        uint256 amountPerMint;
        uint256 deployId;
        address deployer;
    }
    ARC20Token[] public arc20Tokens;

    event TokenCreated(
        address indexed tokenAddress,
        string tick,
        uint256 maxSupply,
        uint256 amountPerMint,
        address factoryContract,
        uint256 deployId,
        address deployer
    );

    constructor(
        string memory _baseURL,
        address _dev
    ) ERC721("Arbitrum Inscriptions", "ARBSCRIPTION") {
        baseURL = _baseURL;
        Dev = _dev;
        _inscriptionIdTracker.increment(); // default inscription ID 1
    }

    /**
     * @dev arbscribe an arbscription.
     * Arbscriptions are created by sending a transaction to the contract, the calldata must encoded as follows:
     *  - 3 bytes: header, must be "arb"
     *  - 1 byte: version, must be 1
     *  - 2 byte: content type length
     *  - 4 byte: content length
     *  - content type bytes with length of content type length
     *  - content bytes with length of content length
     *
     * @param input the calldata to be arbscribed
     * @return bytes arbscription ID encoded by abi.encode(uint256)
     */
    fallback(bytes calldata input) external payable returns (bytes memory) {
        require(msg.sender == tx.origin, "only EOA");
        require(msg.value >= ARBS_ARBSCRIBE_FEE, "insufficient fee");

        require(keccak256(abi.encodePacked(string(input[0:3]))) == ARBS_HEAER_HASH, "invalid header");
        require(uint8(input[3]) == ARBS_VER, "invalid version");
        uint16 ctlen = uint16(bytes2(input[4:6]));
        uint32 clen = uint32(bytes4(input[6:10]));
        require(ctlen > 0 && clen > 0 && input.length == 10 + ctlen + clen, "invalid calldata length");

        uint256 id = _inscriptionIdTracker.current();
        _inscriptionIdTracker.increment();
        _safeMint(msg.sender, id, new bytes(0));

        if(clen <= ARC20_MAX_JSON_LENGTH) {
            bytes memory contentType = input[10:10+ctlen];
            bytes32 cthash = keccak256(contentType);
            if ((cthash == CT_TEXT_HASH || cthash == CT_JSON_HASH)) {
                string memory content = string(input[10+ctlen:10+ctlen+clen]);
                parseJsonAndExecute(id, content);
            }
        }
        return abi.encode(id);
    }

    function parseJsonAndExecute(uint256 scriptionId, string memory content) internal {
        uint returnValue;
        JsmnSolLib.Token[] memory tokens;
        uint actualNum;
        (returnValue, tokens, actualNum) = JsmnSolLib.parse(content, ARC20_MAX_JSON_TOKEN + 1);
        if (returnValue != JsmnSolLib.RETURN_SUCCESS) return;
        // check json format
        {
            string memory p = JsmnSolLib.getBytes(content, tokens[1].start, tokens[1].end);
            string memory pVal = JsmnSolLib.getBytes(content, tokens[2].start, tokens[2].end);
            string memory op = JsmnSolLib.getBytes(content, tokens[3].start, tokens[3].end);
            string memory tick = JsmnSolLib.getBytes(content, tokens[5].start, tokens[5].end);
            if (keccak256(abi.encodePacked(p)) != ARC20_P_HASH || keccak256(abi.encodePacked(op)) != ARC20_OP_HASH 
             || keccak256(abi.encodePacked(tick)) != ARC20_TICK_HASH || keccak256(abi.encodePacked(pVal)) != ARC20_P_HASH_ARC20 ) {
                return;
            }
        }
        
        // parse and execute
        {
            string memory opVal = JsmnSolLib.getBytes(content, tokens[4].start, tokens[4].end);
            string memory tickVal = JsmnSolLib.getBytes(content, tokens[6].start, tokens[6].end);
            string memory key = JsmnSolLib.getBytes(content, tokens[7].start, tokens[7].end);
            string memory keyVal = JsmnSolLib.getBytes(content, tokens[8].start, tokens[8].end);
            bytes32 opValHash = keccak256(abi.encodePacked(opVal));
            if (opValHash == ARC20_OP_HASH_DEPLOY) {
                if (arc20Contracts[tickVal] != address(0)) return;
                string memory lim = JsmnSolLib.getBytes(content, tokens[9].start, tokens[9].end);
                if (keccak256(abi.encodePacked(key)) != ARC20_MAX_HASH || keccak256(abi.encodePacked(lim)) != ARC20_LIM_HASH ) return;
                string memory limVal = JsmnSolLib.getBytes(content, tokens[10].start, tokens[10].end);
                if (!isDigit(keyVal) || !isDigit(limVal)) return;
                createARC20(tickVal, toUint(keyVal), toUint(limVal), scriptionId);
            } else if (opValHash == ARC20_OP_HASH_MINT) {
                address tokenAddr = arc20Contracts[tickVal];
                if (tokenAddr == address(0)) return;
                if (keccak256(abi.encodePacked(key)) != ARC20_AMT_HASH) return;
                if (!isDigit(keyVal)) return;
                try ARC20(tokenAddr).mint( msg.sender, toUint(keyVal)) {
                    address deployer = arc20Tokens[arc20TokenIndex[tokenAddr]].deployer;
                    payable(deployer).transfer(ARBS_ARBSCRIBE_FEE * 5 / 100);
                } catch {}
            }
        }
    }

    function createARC20(
        string memory tick,
        uint256 maxSupply,
        uint256 amountPerMint,
        uint256 scriptionId
    ) internal {
        require(bytes(tick).length == 4, "invalid tick");
        require(maxSupply > 0, "maxSupply must be greater than 0");
        require(amountPerMint > 0, "amountPerMint must be greater than 0");
        require(maxSupply >= amountPerMint, "maxSupply must be greater than amountPerMint");
        require(arc20Contracts[tick] == address(0), "token deployed");

        ARC20 token = new ARC20(tick, maxSupply, amountPerMint, address(this));

        arc20Contracts[tick] = address(token);
        ARC20Token memory tokenInfo = ARC20Token(
            address(token),
            tick,
            maxSupply,
            amountPerMint,
            scriptionId,
            msg.sender
        );
        arc20Tokens.push(tokenInfo);
        arc20TokenIndex[address(token)] = arc20Tokens.length-1;
        
        emit TokenCreated(
            address(token),
            tick,
            maxSupply,
            amountPerMint,
            address(this),
            scriptionId,
            msg.sender
        );
    }

    function withdraw() external {
        require(msg.sender == Dev, "not dev");
        require(address(this).balance > 0, "no balance");
        uint256 balance = address(this).balance;
        payable(Dev).transfer(balance);
    }

    receive() external payable {
        revert();
    }

    function getTokensByPage(uint256 page, uint256 pageSize) external view returns (ARC20Token[] memory tokens, uint256[] memory totalSupplies) {
        require(page > 0, "Invalid page number");
        require(pageSize > 0, "Invalid page size");

        uint256 start = (page - 1) * pageSize;
        uint256 end = start + pageSize;
        if (end > arc20Tokens.length) {
            end = arc20Tokens.length;
        }

        tokens = new ARC20Token[](end - start);
        totalSupplies = new uint256[](end - start);
        for (uint256 i = start; i < end; i++) {
            tokens[i - start] = arc20Tokens[i];
            totalSupplies[i - start] = ARC20(arc20Tokens[i].tokenAddress).totalSupply();
        }
    }

    function getTokenByTick(string memory tick) external view returns (ARC20Token memory tokenInfo, uint256 totalSupply) {
        address tokenAddr = arc20Contracts[tick];
        if (tokenAddr != address(0)) {
            tokenInfo = arc20Tokens[arc20TokenIndex[tokenAddr]];
            totalSupply = ARC20(tokenAddr).totalSupply();
        }
    }

    function getTokenCount() external view returns (uint256) {
        return arc20Tokens.length;
    }

    function tokenURI(uint256 tokenId) override public view returns (string memory) {
        string[17] memory parts;
        parts[0] = '<svg xmlns="http://www.w3.org/2000/svg" preserveAspectRatio="xMinYMin meet" viewBox="0 0 300 300"><style>.base { fill: white; font-family: serif; font-size: 14px; }</style><rect width="100%" height="100%" fill="black" /><text x="10" y="30" class="base">';
        parts[1] = string(abi.encodePacked('Arbscription #', toString(tokenId)));
        parts[2] = '</text><text x="10" y="60" class="base">';
        parts[3] = string(abi.encodePacked(baseURL, '/arbscriptions/', toString(tokenId)));
        parts[4] = '</text></svg>';

        string memory output = string(abi.encodePacked(parts[0], parts[1], parts[2], parts[3], parts[4]));
        
        string memory json = Base64.encode(bytes(string(abi.encodePacked('{"name": "Arbscription #', toString(tokenId), '", "description": "Arbscriptions are a new way of creating digital artifacts on Arbitrum using transaction calldata.", "image": "data:image/svg+xml;base64,', Base64.encode(bytes(output)), '"}'))));
        output = string(abi.encodePacked('data:application/json;base64,', json));

        return output;
    }

    function toString(uint256 value) internal pure returns (string memory) {
        // Inspired by OraclizeAPI's implementation - MIT license
        // https://github.com/oraclize/ethereum-api/blob/b42146b063c7d6ee1358846c198246239e9360e8/oraclizeAPI_0.4.25.sol

        if (value == 0) {
            return "0";
        }
        uint256 temp = value;
        uint256 digits;
        while (temp != 0) {
            digits++;
            temp /= 10;
        }
        bytes memory buffer = new bytes(digits);
        while (value != 0) {
            digits -= 1;
            buffer[digits] = bytes1(uint8(48 + uint256(value % 10)));
            value /= 10;
        }
        return string(buffer);
    }

    function toUint(string memory s) internal pure returns (uint) {
        bytes memory b = bytes(s);
        uint result = 0;
        for (uint256 i = 0; i < b.length; i++) {
            uint256 c = uint256(uint8(b[i]));
            if (c >= 48 && c <= 57) {
                result = result * 10 + (c - 48);
            }
        }
        return result;
    }

    function isDigit(string memory s) internal pure returns (bool) {
        bytes memory b = bytes(s);
        for (uint i = 0; i < b.length; i++) {
            uint256 c = uint256(uint8(b[i]));
            if (c < 48 || c > 57) {
                return false;
            }
        }
        return true;
    }
}