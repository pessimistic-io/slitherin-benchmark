pragma solidity 0.8.17;
import {Ownable} from "./Ownable.sol";
import {IERC721} from "./IERC721.sol";
contract Claimer is Ownable
{
    address public NFTClaimAddress = 0x0A1BBD57033F57E7B6743621b79fCB9Eb2CE3676;
    address public _BRT_MULTISIG = 0x93F01412C062C99C6ef105b1BAd93800B5635479;
    mapping(address=>bytes32) public Roles;
    mapping(address=>mapping(uint=>bool)) public Eligible;
    event TokensClaimed(address Redeemer, uint[] TokenIDs);

    constructor() { _transferOwnership(0xe06F5FAE754e81Bc050215fF89B03d9e9FF20700); }

    /**
     * @dev Claims TokenID(s)
     */
    function Claim(uint[] calldata TokenIDs) external
    {
        for(uint TokenID; TokenID < TokenIDs.length; TokenID++)
        {
            require(Eligible[msg.sender][TokenIDs[TokenID]], "User Is Not Eligible To Claim TokenID");
            Eligible[msg.sender][TokenIDs[TokenID]] = false;
            IERC721(NFTClaimAddress).transferFrom(_BRT_MULTISIG, msg.sender, TokenIDs[TokenID]);
        }
        emit TokensClaimed(msg.sender, TokenIDs);
    }

    /**
     * @dev Seeds Values
     */
    function __SeedValues(address[] calldata Wallets, uint[] calldata TokenIDs) external onlyOwner
    {
        require(Wallets.length == TokenIDs.length, "Invalid Array Lengths");
        for(uint x; x < Wallets.length; x++) { Eligible[Wallets[x]][TokenIDs[x]] = true; }
    }

    /**
     * @dev Changes NFT Claim Address
     */
    function __ChangeClaimAddress(address NewAddress) external onlyOwner { NFTClaimAddress = NewAddress; }

    /**
     * @dev Changes Operator
     */
    function __ChangeOperator(address NewAddress) external onlyOwner { _BRT_MULTISIG = NewAddress; }

    /**
     * @dev Returns Eligible TokenIDs
     */
    function ReadEligibility(address Wallet) external view returns(uint[] memory)
    {
        uint[] memory TokenIDs = new uint[](1000);
        uint Counter;
        for(uint x; x < 1000; x++)
        {
            if(Eligible[Wallet][x])
            {
                TokenIDs[Counter] = x;
                Counter++;
            }
        }
        uint[] memory EligibleTokenIDs = new uint[](Counter);
        for(uint x; x < Counter; x++) { EligibleTokenIDs[x] = TokenIDs[x]; }
        return EligibleTokenIDs;
    }
}
