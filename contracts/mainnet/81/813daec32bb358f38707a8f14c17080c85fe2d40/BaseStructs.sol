pragma solidity ^0.8.13;

import {TokenType} from "./TokenEnums.sol";

enum SignatureVersion {
    EIP712,
    EIP1271
}

struct NFT {
    TokenType tokenType;
    address token;
    uint256 tokenId;
    uint256 amount;
}

struct Fee {
    uint16 rate;
    address payable recipient;
}

struct Signature {
    bytes signature;
    SignatureVersion signatureVersion;
}

struct Metadata {
    bytes32 metadataHash;
    address checker;
}

