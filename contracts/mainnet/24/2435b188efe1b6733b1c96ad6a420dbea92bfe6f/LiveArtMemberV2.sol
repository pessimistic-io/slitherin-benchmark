contract LiveArtMemberV2 is LiveArtMember {
    uint256 public claimEndDate;

    event SetClaimEndDate(uint256 indexed timestamp);

    function setEndDateTimestamp(uint256 _claimEndDate) external onlyOwner {
        claimEndDate = _claimEndDate;

        emit SetClaimEndDate(_claimEndDate);
    }

    function claim(
        uint256 index,
        uint256 amount,
        bytes32[] calldata merkleProof
    ) public override returns (uint256) {
        require(block.timestamp <= claimEndDate, "LiveArtMember: end");

        return super.claim(index, amount, merkleProof);
    }
}

